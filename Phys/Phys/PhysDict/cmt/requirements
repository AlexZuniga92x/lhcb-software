package Phys/PhysDict
version v2r2
# =============== structure ===================================================
branches         cmt doc dict
# =============== dependencies ================================================
use              GaudiPython v* 
use              MCEvent     v* Event
use              RecEvent    v* Event
use              PhysEvent   v* Event
use              DaVinciKernel v* Phys
use              DaVinciMCKernel v* Phys
use              DaVinciMCTools v* Phys

# =============== LCG Dictionaries ============================================
apply_pattern     reflex_dictionary                            \
                  dictionary=Phys                               \
                  headerfiles=$(PHYSDICTROOT)/dict/PhysDict.h     \
                  selectionfile=$(PHYSDICTROOT)/dict/PhysDict.xml \
                  options="-U__MINGW32__"

macro PhysDict_use_linkopts $(use_linkopts)

