package AlignmentSys
version v4r2

branches cmt doc
 
#=======================================================
# LHCb Alignment packages
#=======================================================

# General purpose components
use AlignmentInterfaces	v3r0	Alignment # Johan Blouw
use AlignmentTools      v3r0    Alignment # Sebastien Viret
use AlignEvent		v1r0    Alignment # Wouter Hulsbergen
use AlignKernel		v3r0    Alignment # Adlene Hicheur, Wouter Hulsbergen
use AlignSolvTools	v3r0	Alignment # A. Hicheur, W. Hulsbergen
use AlignTrTools	v4r3	Alignment # Johan Blouw, Adlene Hicheur, Louis Nicolas
use TAlignment		v4r3	Alignment # Jan Amoraal, Johan Blouw, Marc Deissenroth, Florin Maciuc
use AlignT2Velo         v1r1    Alignment # Steve Blusk
use VeloAlignment	v4r7	Alignment # Marco Gersabeck
use MisAligner		v1r1	Alignment # Johan Blouw, Manuel Schiller
use Escher		v4r3	Alignment # Johan Blouw

#=======================================================
# LHCb Calibration packages
#=======================================================

use OTCalibration	v1r0	Calibration # Wouter Hulsbergen


# Allow the generation of QMTest summary 
apply_pattern QMTestSummarize

#============================================================================
# Install the configurable
apply_pattern install_python_modules

