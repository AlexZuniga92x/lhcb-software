package           LbcomSys
version           v13r4

branches doc cmt tests

#===============================================================
# LHCb packages shared by Boole and other applications
#===============================================================

# Common
use Associators       v3r8     Associators # Matt Needham

# Calorimeter
use CaloAssociators   v6r12    Calo      # Olivier Deschamps
use CaloTools         v6r5     Calo      # Olivier Deschamps

# DAQ
use DAQMonitors       v1r9     DAQ       # Olivier Deschamps

# Fibre tracker
use FTDAQ             v1r2     FT        # Olivier Callot

# HLT
use HltDAQ            v2r16    Hlt       # Tomasz Skwarnicki

# L0 trigger simulation
use L0Calo            v11r9    L0        # Patrick Robbe
use L0DU              v10r27   L0        # Olivier Deschamps   
use L0Muon            v9r12    L0        # Julien Cogan
use L0MuonMonitor     v2r3     L0        # Julien Cogan
use PuVeto            v7r9     L0        # Serena Oggero

# Muon
use MuonAssociators   v3r5     Muon      # Alessia Satta

# Outer tracker
use OTAssociators     v4r6     OT        # Alexandr Kozlinskiy
use OTMonitor         v3r5     OT        # Wouter Hulsbergen

# Rich
use RichAlgorithms    v1r6     Rich      # Chris Jones
use RichDAQ           v3r39    Rich      # Chris Jones
use RichMCMonitors    v1r10    Rich      # Chris Jones
use RichMCTools       v3r18    Rich      # Chris Jones
use RichTools         v2r41    Rich      # Chris Jones

# Silicon detectors
use SiTools           v1r9     Si        # Matt Needham

# Silicon Tracker
use STAlgorithms      v4r6     ST        # Jeroen van Tilburg, Matt Needham
use STAssociators     v1r7     ST        # Jeroen van Tilburg, Matt Needham
use STCheckers        v4r1     ST        # Jeroen van Tilburg, Matt Needham
use STDAQ             v4r2     ST        # Jeroen van Tilburg, Matt Needham
use STMonitors        v4r2     ST        # Mark Tobin
use STTools           v4r14    ST        # Jeroen van Tilburg, Matt Needham

# Velo
use VeloAlgorithms    v5r7     Velo      # Tomasz Szumlak
use VeloAssociators   v2r5     Velo      # Tomasz Szumlak
use VeloDAQ           v6r9     Velo      # Kurt Rinnert
use VeloSimulation    v3r14    Velo      # Tomasz Szumlak
use VeloTools         v4r2     Velo      # Tomasz Szumlak

# VeloLite
use VLDAQ             v1r3     VL        # Heinrich Schindler

# Velo Pixel
use VPDAQ             v2r1     VP        # Marcin Kucharczyk
use VPTools           v2r1     VP        # Victor Coco

# Declare this as a container package
apply_pattern container_package

# Allow the generation of QMTest summary 
apply_pattern QMTestSummarize

# Allow the generation of QMTests
apply_pattern QMTest
