package LbcomSys
version v9r5

branches doc cmt tests

#===============================================================
# LHCb packages shared by Boole and other applications
#===============================================================

# Common
use Associators       v3r7    Associators # Matt Needham

# Calorimeter
use CaloAssociators   v6r10p1 Calo      # In SVN. Olivier Deschamps
use CaloTools         v5r16p1 Calo      # In SVN. Olivier Deschamps

# DAQ
use DAQMonitors       v1r2    DAQ       # In SVN. Olivier Deschamps

# HLT
use HltDAQ            v2r0p2  Hlt       # Tomasz Skwarnicki

# L0 trigger simulation
use L0Calo            v11r2p1 L0        # Olivier Deschamps, Patrick Robbe
use L0DU              v10r8   L0        # Olivier Deschamps   
use L0Muon            v9r1p1  L0        # Andrei Tsarogorodtsev, Julien Cogan
use L0MuonMonitor     v2r0p1  L0        # Julien Cogan
use PuVeto            v7r7p1  L0        # Serena Oggero

# Muon
use MuonAssociators   v3r3    Muon      # Alessia Satta

# Outer tracker
use OTAssociators     v4r4    OT        # Alexandr Kozlinskiy
use OTMonitor         v3r2p1  OT        # Wouter Hulsbergen

# Rich
use RichAlgorithms    v1r2    Rich      # Chris Jones
use RichDAQ           v3r27   Rich      # Chris Jones
use RichMCMonitors    v1r7p2  Rich      # Chris Jones
use RichMCTools       v3r13   Rich      # Chris Jones
use RichTools         v2r30   Rich      # Chris Jones

# Silicon detectors
use SiTools           v1r7p1  Si        # Matt Needham

# Silicon Tracker
use STAlgorithms      v4r2    ST        # Jeroen van Tilburg, Matt Needham
use STAssociators     v1r6    ST        # Jeroen van Tilburg, Matt Needham
use STCheckers        v3r1    ST        # Jeroen van Tilburg, Matt Needham
use STDAQ             v3r22   ST        # Jeroen van Tilburg, Matt Needham
use STMonitors        v3r2    ST        # Mark Tobin
use STTools           v4r3    ST        # Jeroen van Tilburg, Matt Needham

# Velo
use VeloAlgorithms    v5r2    Velo      # Tomasz Szumlak
use VeloAssociators   v2r4    Velo      # Tomasz Szumlak
use VeloDAQ           v5r3    Velo      # Kurt Rinnert
use VeloSimulation    v3r7    Velo      # Tomasz Szumlak
use VeloTools         v3r5    Velo      # Tomasz Szumlak

# VeloPix
use VeloPixDAQ        v1r2p1  VeloPix  # Marcin Kucharczyk
use VeloPixTools      v1r0p1  VeloPix  # In SVN. Victor Coco

# Declare this as a container package
apply_pattern container_package

# Allow the generation of QMTest summary 
apply_pattern QMTestSummarize

# Allow the generation of QMTests
apply_pattern QMTest
