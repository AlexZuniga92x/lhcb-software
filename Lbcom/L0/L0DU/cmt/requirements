package		L0DU
version 	v1

# ================ structure =============================================
branches	L0DU src cmt doc options

# ================ own includes ==========================================
include_dirs	${L0DUROOT}

# ================ options ===============================================
set L0DUOPTS	${L0DUROOT}/options

# ================ used packages =========================================
use L0Calo	v1*	L0
use L0Muon      v1*     L0

# ================ component library =====================================
library		L0DU	../src/*.cpp

apply_pattern	package_Clinkopts    
apply_pattern	packageShr

#================= private stuff =========================================
private
apply_pattern	package_Cshlibflags
ignore_pattern	package_stamps

