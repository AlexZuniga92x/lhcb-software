package           PuVeto
version           v7r0

# Structure, i.e. directories to process. 
#============================================================================
branches          cmt doc src options
include_path none

# Used packages. Specify the version, * at the end specifies 'any revision'
#============================================================================
use	L0Event		v15r*	Event
use	VeloEvent	v14r*	Event
use     DAQEvent        v6r*    Event
use     VeloDet         v10r*   Det
use	MCEvent		v1r*	Event

# Component library building rule
#============================================================================
library          PuVeto    ../src/*.cpp

apply_pattern component_library library=PuVeto
