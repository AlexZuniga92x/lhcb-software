package          BenderUser
author           Vanya BELYAEV
version          v4r0

# ============== structure ===================================================
branches         cmt doc python 

# ============== environment =================================================
set              BENDERUSERPYTHON  $(BENDERUSERROOT)/python

# ============== dependencies ================================================
use              Bender      v*   Phys 
use              BenderVis   v*   Vis

# ============== nesessary paths =============================================
path_remove      PYTHONPATH "/BenderUser/"  WIN32  "\BenderUser\"
path_prepend     PYTHONPATH "$(BENDERUSERROOT)/python"

# ============== patterns =====================================================
apply_pattern    application_path

