package          BenderExample
author           Vanya BELYAEV
version          v2r0

# ============== structure ===================================================
branches         cmt doc src python options

# ============== environment =================================================
set              BENDEREXAMPLEPYTHON  $(BENDEREXAMPLEROOT)/python
set              BENDEREXAMPLEOPTS    $(BENDEREXAMPLEROOT)/options

# ============== dependencies ================================================
use              Bender      v*   Phys 

# ============== nesessary paths =============================================
path_remove      PYTHONPATH "/BenderExample/"  WIN32  "\BenderExample\"
path_prepend     PYTHONPATH "$(BENDEREXAMPLEROOT)/python"

path_remove      PYTHONPATH "hippodraw" 
path_append      PYTHONPATH  "/afs/cern.ch/sw/lcg/external/hippodraw/1.3.3/rh73_gcc32/lib"

# ============== patterns =====================================================
apply_pattern    application_path

