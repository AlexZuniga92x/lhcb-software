package          BenderExample
author           Vanya BELYAEV
version          v1r0

# ============== structure ===================================================
branches         cmt doc src python options scripts 

# ============== environment =================================================
set              BENDEREXAMPLEOPTS    $(BENDEREXAMPLEROOT)/options 
set              BENDEREXAMPLESRC     $(BENDEREXAMPLEROOT)/src 
set              BENDEREXAMPLEPYTHON  $(BENDEREXAMPLEROOT)/python

# ============== dependencies ================================================
use              Bender      v*    Tools

# ============== nesessary paths =============================================
path_remove      PYTHONPATH "/BenderExample/"  WIN32  "\BenderExample\"
path_prepend     PYTHONPATH "$(BENDEREXAMPLEROOT)/src"
path_prepend     PYTHONPATH "$(BENDEREXAMPLEROOT)/python"

path_remove      PATH       "/BenderExample/"  WIN32  "\BenderExample\"
path_prepend     PATH       "$(BENDEREXAMPLEROOT)/scripts"

# ============================================================================
# The END 
# ============================================================================
# $Log: not supported by cvs2svn $
