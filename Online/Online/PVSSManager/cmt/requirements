package PVSSManager
version v1r0

branches  cmt doc
use PVSS v* Online  -no_auto_imports

apply_pattern install_more_includes more=PVSSManager offset=PVSS

library PVSSManager *.cpp -import=PVSS
apply_pattern linker_library library=PVSSManager
macro_append PVSSManager_shlibflags $(PVSS_linkopts)

private
application PVSS00python  ../main/*.cpp
macro_append PVSS00python_cppflags " -m32 " WIN32 ""
macro_append PVSS00pythonlinkopts " -m32  $(PVSS_linkopts) " WIN32 ""
