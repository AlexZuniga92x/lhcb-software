MBM.PartitionBuffers                 = @OnlineEnv.PartitionBuffers;
MBM.PartitionID                      = @OnlineEnv.PartitionID;
MBM.PartitionName                    = @OnlineEnv.PartitionName;
MBM.Buffers                          = @OnlineEnv.DiskWR_Buffers;
