ApplicationMgr.ExtSvc               += {"LHCb::OnlineEvtSelector/EventSelector"};
ApplicationMgr.SvcOptMapping	     = {"LHCb::OnlineRunable/EmptyEventLoop",
                                        "LHCb::OnlineRunable/Runable",
                                        "LHCb::RawDataCnvSvc/RawDataCnvSvc"};
