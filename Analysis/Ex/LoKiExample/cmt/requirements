package         LoKiExample
version         v3r3p2

# ============= structure =====================================================
branches        cmt doc src jobs options 

# ============= environment ===================================================
set LOKIEXAMPLEDOC    $(LOKIEXAMPLEROOT)/doc 
set LOKIEXAMPLEOPTS   $(LOKIEXAMPLEROOT)/options
set LOKIEXAMPLEJOBS   $(LOKIEXAMPLEROOT)/options

# ============= dependencies  =================================================
use             LoKi   v* Tools
use             Python v* LCG_Interfaces -no_auto_imports 

# ============= constituents ==================================================
library         LoKiExample  *.cpp 

# ============= patterns ======================================================
apply_pattern component_library library=LoKiExample 

# ============= paths =========================================================
path_remove PYTHONPATH  "LoKiExample" 
path_append PYTHONPATH  ${LOKIEXAMPLEJOBS}
path_remove PATH        "LoKiExample" 
path_append PATH        ${LOKIEXAMPLEJOBS}

