StdLooseLambda1115_LL.MotherFilter.Selections = 
                                       {"Lambda0 : VtxFilterCriterion,FlightDistanceFilterCriterion"};
StdLooseLambda1115_LL.DaughterFilter.Selections =
                                       {"p+ : KinFilterCriterion/protonKin, 
                                              PVIPFilterCriterion/protonPVIP",
                                        "pi- : KinFilterCriterion/pionKin,
                                               PVIPFilterCriterion/pionPVIP"}; 
StdLooseLambda1115_LL.KillOverLap = true;
StdLooseLambda1115_LL.MotherFilter.FlightDistanceFilterCriterion.CutBestPV = false;


