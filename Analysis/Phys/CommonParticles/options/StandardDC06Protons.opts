/** @file 
 *
 *  The configuration file for "StandardDC06" protons, created "on-demand"
 * 
 * The configuration includes:
 *    - "/Event/Phys/StdDC06NoPIDsProtons"  
 *    - "/Event/Phys/StdDC06LooseProtons"
 *    - "/Event/Phys/StdDC06LooseVTTProtons"
 *    - "/Event/Phys/StdDC06LooseDownstreamProtons"
 *    - "/Event/Phys/StdDC06TightProtons"
 *
 *  @author Vanya BELYAEV ibelyaev@physics.syr.edu
 *  @author Juan PALACIOS juan.palacios@cern.ch
 *  @date 2006-06-27
 */
DataOnDemandSvc.Algorithms += {
  // No PID protons 
  "DATA='/Event/Phys/StdDC06NoPIDsProtons/Particles'   TYPE='GaudiSequencer/StdDC06NoPIDsProtonsSeq'",
  "DATA='/Event/Phys/StdDC06NoPIDsProtons/Vertices'    TYPE='GaudiSequencer/StdDC06NoPIDsProtonsSeq'",
  // Loose protons:    RICH & DLL(p-pi)>-5
  "DATA='/Event/Phys/StdDC06LooseProtons/Particles'   TYPE='GaudiSequencer/StdDC06LooseProtonsSeq'",
  "DATA='/Event/Phys/StdDC06LooseProtons/Vertices'    TYPE='GaudiSequencer/StdDC06LooseProtonsSeq'",
  // Loose VTT protons:    RICH & DLL(p-pi)>-5
  "DATA='/Event/Phys/StdDC06LooseVTTProtons/Particles'   TYPE='GaudiSequencer/StdDC06LooseVTTProtonsSeq'",
  "DATA='/Event/Phys/StdDC06LooseVTTProtons/Vertices'    TYPE='GaudiSequencer/StdDC06LooseVTTProtonsSeq'",
  // Loose Downstream protons:    RICH & DLL(p-pi)>-5
  "DATA='/Event/Phys/StdDC06LooseDownstreamProtons/Particles'   TYPE='GaudiSequencer/StdDC06LooseDownstreamProtonsSeq'",
  "DATA='/Event/Phys/StdDC06LooseDownstreamProtons/Vertices'    TYPE='GaudiSequencer/StdDC06LooseDownstreamProtonsSeq'",
  // Tight protons
  "DATA='/Event/Phys/StdDC06TightProtons/Particles'   TYPE='GaudiSequencer/StdDC06TightProtonsSeq'",
  "DATA='/Event/Phys/StdDC06TightProtons/Vertices'    TYPE='GaudiSequencer/StdDC06TightProtonsSeq'"
};
StdDC06NoPIDsProtonsSeq.Members += { "NoPIDsParticleMaker/StdDC06NoPIDsProtons" };
StdDC06LooseProtonsSeq.Members += { "CombinedParticleMaker/StdDC06LooseProtons" };
StdDC06LooseVTTProtonsSeq.Members += { "CombinedParticleMaker/StdDC06LooseVTTProtons" };
StdDC06LooseDownstreamProtonsSeq.Members += { "CombinedParticleMaker/StdDC06LooseDownstreamProtons" };
StdDC06TightProtonsSeq.Members += { "CombinedParticleMaker/StdDC06TightProtons" };

// ============================================================================
// NO PID, all tracks
// ============================================================================
StdDC06NoPIDsProtons.Particle =  "proton" ;
StdDC06NoPIDsProtons.DecayDescriptor = "Proton" ;
// ============================================================================
// LOOSE long (except VTT protons)
// ============================================================================
StdDC06LooseProtons.Particle = "proton" ;
StdDC06LooseProtons.Proton.Selection = {"RequiresDet='RICH' CombDLL(p-pi)>'-5.0'" } ;
StdDC06LooseProtons.TrackSelector.TrackTypes = {"Long"};

StdDC06LooseDownstreamProtons.Particle = "proton" ;
StdDC06LooseDownstreamProtons.Proton.Selection = {"RequiresDet='RICH' CombDLL(p-pi)>'-5.0'" } ;
StdDC06LooseDownstreamProtons.TrackSelector.TrackTypes = {"Downstream"} ;

StdDC06LooseVTTProtons.Particle = "proton" ;
StdDC06LooseVTTProtons.Proton.Selection = {"RequiresDet='RICH' CombDLL(p-pi)>'-5.0'" } ;
StdDC06LooseVTTProtons.TrackSelector.TrackTypes={"Upstream"} ;

// ============================================================================
// TIGHT
// ============================================================================

StdDC06TightProtons.Particle = "proton" ;
StdDC06TightProtons.Proton.Selection = {"RequiresDet='RICH' CombDLL(p-pi)>'0.0'" } ;
StdDC06TightProtons.TrackSelector.TrackTypes = {"Long"};
StdDC06TightProtons.DecayDescriptor = "Proton" ;

// ============================================================================
// The END 
// ============================================================================
