//STRIP OPTIONS PreselBs2GammaMuMu  DimuonForPreselBs2GammaMuMu PreselBs2GammaMuMu
// From DVPreselBs2GammaMuMu.opts:
//In ->DVPreselBs2GammaMuMu->DoPreselBs2GammaMuMu
ApplicationMgr.TopAlg += { "GaudiSequencer/SeqPreselBs2GammaMuMu" } ;
SeqPreselBs2GammaMuMu.Members += { "MakeResonances/DimuonForPreselBs2GammaMuMu", "MakeResonances/PreselBs2GammaMuMu", "PrintHeader/PrintPreselBs2GammaMuMu" } ;
//In ->DVPreselBs2GammaMuMu->DoPreselBs2GammaMuMu->DimuonForPreselBs2GammaMuMu
DimuonForPreselBs2GammaMuMu.InputLocations  = { "Phys/StdDC06TightMuons" } ;
DimuonForPreselBs2GammaMuMu.DecayDescriptors  = { "J/psi(1S) -> mu+ mu-" } ;
DimuonForPreselBs2GammaMuMu.Window  = 3000.0*MeV ;
DimuonForPreselBs2GammaMuMu.MotherFilter.Selections  = { "J/psi(1S) :       VtxFilterCriterion" } ;
DimuonForPreselBs2GammaMuMu.MotherFilter.VtxFilterCriterion.MaxChi2  = 14.0 ;
DimuonForPreselBs2GammaMuMu.DaughterFilter.Selections  = { "mu+ : KinFilterCriterion/muPKin" } ;
DimuonForPreselBs2GammaMuMu.DaughterFilter.muPKin.MinPt  = 500*MeV ;
//In ->DVPreselBs2GammaMuMu->DoPreselBs2GammaMuMu->PreselBs2GammaMuMu
PreselBs2GammaMuMu.InputLocations  = { "Phys/DimuonForPreselBs2GammaMuMu", "Phys/StdDC06LooseAllPhotons" } ;
PreselBs2GammaMuMu.DecayDescriptor  = "B_s0 -> gamma J/psi(1S)" ;
PreselBs2GammaMuMu.Window  = 1000.0*MeV ;
PreselBs2GammaMuMu.MotherFilter.Selections  = { "B_s0 :  PVIPFilterCriterion,
                                                      Momentum2FlightAngleFilterCriterion" } ;
PreselBs2GammaMuMu.MotherFilter.PVIPFilterCriterion.MaxIPsignif  = 6.0 ;
PreselBs2GammaMuMu.MotherFilter.Momentum2FlightAngleFilterCriterion.CosAngle  = 0.9995 ;
PreselBs2GammaMuMu.DaughterFilter.Selections  = { "gamma  : KinFilterCriterion/GammaKin" } ;
PreselBs2GammaMuMu.DaughterFilter.GammaKin.MinPt  = 1000*MeV ;
//Added:
SeqPreselBs2GammaMuMu.MeasureTime  = true ;
PrintPreselBs2GammaMuMu.OutputLevel  = 3 ;



//STRIP OPTIONS PreselBd2Kstaree  DiLeptonForBd2LLKstar Kstar2KPiForBd2LLKstar PreselBd2Kstaree
