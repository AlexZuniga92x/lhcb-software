StdMCMuons.PhysDesktop.MCParticleMaker.IpErrorZ = 0.001;   //mm=1
StdMCMuons.PhysDesktop.MCParticleMaker.SlopeError = 0.0004;  //mrad=0.001
StdMCMuons.PhysDesktop.MCParticleMaker.MomError = 0.004;
StdMCMuons.PhysDesktop.MCParticleMaker.rhoxtx = 0.;
StdMCMuons.PhysDesktop.MCParticleMaker.rhoyty = 0.;
