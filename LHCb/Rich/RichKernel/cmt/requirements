package RichKernel
version v4r5

# ================ structure =================================================
branches RichKernel cmt doc src

# ================ environment ===============================================
include_dirs $(RICHKERNELROOT)

# ================ dependencies ==============================================
use LHCbKernel	v4r*  Kernel

# ================ linker library ============================================
library RichKernel ../src/*.cpp
apply_pattern linker_library library=RichKernel
