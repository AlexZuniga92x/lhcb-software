# ============= pyqt_resource_header:begin =============
inputs = $(qrc_files)

${CONSTITUENT} :: $(output)

${CONSTITUENT}clean ::
	$(cmt_uninstallarea_command) $(output)

$(output):: $(inputs)
        echo $(inputs)
	$(pyqt_resource_command) -o $(output) $(inputs)
# ============= pyqt_resource_header:end =============
