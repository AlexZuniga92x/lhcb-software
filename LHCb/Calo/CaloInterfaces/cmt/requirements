# =============================================================================
package           CaloInterfaces
version           v4r0

# =============== structure ===================================================
branches          cmt doc src CaloInterfaces

# =============== include directories =========================================
include_dirs      $(CALOINTERFACESROOT)

# =============== dependencies ================================================
use LHCbKernel    v5r*	Kernel
  
# =============== build the library ===========================================
library           CaloInterfaces *.cpp 
apply_pattern     linker_library library=CaloInterfaces 


