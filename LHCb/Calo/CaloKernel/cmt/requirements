package           CaloKernel
version           v2r3

# =============== structure ===================================================
branches          src cmt doc CaloKernel

# =============== own includes ================================================
include_dirs      $(CALOKERNELROOT)

# =============== used packages ===============================================
use LHCbKernel    v2r*  Kernel
use GaudiKernel   v13r*
use CLHEP         v* 
        
# =============== linker library ==============================================
library           CaloKernel            *.cpp 
apply_pattern     package_stamps 

# =============== mandatory patterns ==========================================
apply_pattern     linker_library library=CaloKernel

