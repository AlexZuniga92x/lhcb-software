package           CaloKernel
version           v3r1

# =============== structure ===================================================
branches          src cmt doc CaloKernel

# =============== own includes ================================================
include_dirs      $(CALOKERNELROOT)

# =============== used packages ===============================================
use LHCbKernel    v5r*  Kernel
use GaudiAlg	  v*
        
# =============== linker library ==============================================
library           CaloKernel            *.cpp 

# =============== mandatory patterns ==========================================
apply_pattern     linker_library library=CaloKernel

