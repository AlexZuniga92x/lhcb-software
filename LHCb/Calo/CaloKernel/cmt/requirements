package           CaloKernel
version           v3r0

# =============== structure ===================================================
branches          src cmt doc CaloKernel

# =============== own includes ================================================
include_dirs      $(CALOKERNELROOT)

# =============== used packages ===============================================
use LHCbKernel    v4r*  Kernel

use GaudiAlg   v7r0 
use GaudiTools v8r0 
        
# =============== linker library ==============================================
library           CaloKernel            *.cpp 

# =============== mandatory patterns ==========================================
apply_pattern     linker_library library=CaloKernel

