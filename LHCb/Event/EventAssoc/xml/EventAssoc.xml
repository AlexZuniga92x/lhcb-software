<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE gdd SYSTEM "gdd.dtd"[]>

<gdd>

<package name="EventAssoc">

    <assoc from="ITDigit"   to="MCParticle"/>
    <assoc from="ITDigit"   to="MCHit"      weight="double"/>
    <assoc from="ITCluster" to="MCParticle" weight="double"/>
    <assoc from="ITCluster" to="MCHit"      weight="double"/>

    <import name="Event/OTTime"/>
    <assoc from="OTTime"    to="MCHit"      type="1D"/>

    <assoc from="MuonCoord" to="MCParticle"/>

    <assoc from="TrStoredTrack" to="float"/>
    <assoc from="TrStoredTrack" to="MCParticle" weight="double"/>

    <assoc from="L0CaloCandidate" to="MCParticle" weight="double"/>

    <assoc from="CaloDigit"   to="MCParticle"    weight="float"/>
    <assoc from="CaloCluster" to="MCParticle"    weight="float"/>
    <assoc from="CaloCluster" to="TrStoredTrack" weight="float" type="2D"/>
    <assoc from="CaloHypo"    to="TrStoredTrack" weight="float" type="2D"/>

    <assoc from="VeloCluster" to="MCParticle" weight="double" type="2D"/>
    <assoc from="VeloCluster" to="MCVeloHit"  weight="double" type="2D"/>

    <assoc from="ProtoParticle" to="MCParticle" weight="double"/>

    <assoc from="Particle" to="MCParticle" weight="double"/>
    <assoc from="Particle" to="MCParticle"/>

    <assoc from="Particle" to="FlavourTag" type="1D"/>
    <assoc from="Particle" to="FlavourTag" type="2D"/>
    <assoc from="Particle" to="Vertex" weight="float"/>
    <assoc from="Particle" to="Vertex" weight="float" type="2D"/>
</package>

</gdd>
