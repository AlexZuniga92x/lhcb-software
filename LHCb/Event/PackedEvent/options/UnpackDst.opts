// Job options to configure the data on demand service for unpacking the
// packed containers produced by the reconstruction

// Declare the DLL, and add MC truth containers 
#include "$PACKEDEVENTROOT/options/UnpackSim.opts"

// Set up the algorithms
DataOnDemandSvc.Algorithms += {
  "DATA='/Event/Rec/Tr/Best'        TYPE='UnpackTrStoredTrack'"
, "DATA='/Event/Rec/Tr/StatesP'     TYPE='UnpackTrStateP'"
, "DATA='/Event/Rec/Tr/StatesL'     TYPE='UnpackTrStateL'"
};
