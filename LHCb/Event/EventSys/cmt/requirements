package EventSys
version v20r2

branches doc cmt

# LHCb Event libraries

use Event       v4r7    Event
use VeloEvent   v13r2   Event
use ITEvent     v13r2p1 Event
use OTEvent     v14r0p1 Event
use TrEvent     v14r2   Event
use RichEvent   v8r3    Event
use CaloEvent   v13r9   Event
use MuonEvent   v4r0    Event
use L0Event     v14r3   Event
use PhysEvent   v8r9    Event
use DAQEvent    v5r0    Event
use GenEvent    v3r3p1  Event
use EventAssoc  v1r4    Event
use LinkerEvent v1r7    Event
use TrgEvent    v5r4    Event

private

# Check completeness of LCG dictionaries. Only for Linux debug

pattern dict_check \
  use GaudiPoolDb v* ; \
  apply_pattern ld_library_path ; \
  make_fragment dict_check -header=dict_check_header ; \
  document dict_check LCG_dict_check

macro apply_check "" \
      Linux&debug dict_check

apply_pattern ${apply_check}
