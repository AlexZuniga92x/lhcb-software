package EventSys
version v19r3

branches doc cmt

# LHCb Event libraries

use Event       v4r6    Event
use VeloEvent   v13r2   Event
use ITEvent     v13r2   Event
use OTEvent     v14r0   Event
use TrEvent     v14r1   Event
use RichEvent   v8r1    Event
use CaloEvent   v13r8   Event
use MuonEvent   v3r2    Event
use L0Event     v14r2   Event
use PhysEvent   v8r7    Event
use DAQEvent    v4r5    Event
use GenEvent    v3r3    Event
use EventAssoc  v1r4    Event
use LinkerEvent v1r5    Event
use TrgEvent    v5r3    Event

private

# Check completeness of LCG dictionaries. Only for Linux debug

pattern dict_check \
  use GaudiPoolDb v* ; \
  apply_pattern ld_library_path ; \
  make_fragment dict_check -header=dict_check_header ; \
  document dict_check LCG_dict_check

macro apply_check "" \
      Linux&debug dict_check

apply_pattern ${apply_check}
