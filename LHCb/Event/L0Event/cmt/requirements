package L0Event
version v10r0

branches Event src cmt doc xml

include_dirs $(L0EVENTROOT)

use 	GaudiObjDesc	v3r*
use     EventKernel     v1r*    Event
use 	LHCbEvent 	v12r* 	Event

# component library

document obj2doth L0EventObj2Doth ../xml/*.xml

library         L0Event ../src/*.cpp 

document obj2dict L0EventObj2Dict -group=dict ../xml/L0Event.xml
library L0EventDict -group=dict ../dict/*.cpp
macro L0EventDict+shlibflag "$(use_linkopts) $(libraryshr_linkopts)"

apply_pattern   package_Cshlibflags
apply_pattern   package_Clinkopts
apply_pattern   packageShr



 