package DetSys
version v8r10

branches cmt doc dict

use DetDesc             v16r6   Det
use DetDescCnv		v3r3    Det
use DetDescSvc          v1r5    Det
use XmlTools		v3r10   Tools
use VeloDet        	v10r8   Det
use OTDet               v6r5    Det
use CaloDet	   	v7r4    Det
use CaloDetXmlCnv  	v2r3p2	Det
use MuonDet             v5r4    Det
use Magnet              v3r3    Det
use RichDet	   	v9r5    Det
use STDet          	v2r5    Det

# =============== LCG Dictionaries ============================================
apply_pattern     reflex_dictionary                            \
                  dictionary=Det                               \
                  headerfiles=$(DETSYSROOT)/dict/DetDict.h     \
                  selectionfile=$(DETSYSROOT)/dict/DetDict.xml \
                  options="-U__MINGW32__"

# =============== patterns ====================================================
apply_pattern     component_library    library=DetDict
