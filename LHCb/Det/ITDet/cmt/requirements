package ITDet
version v1r7

branches cmt doc src options

include_dirs $(ITDETROOT)

use DetDescCnv v2r* Det
use LHCbKernel	v2r*	Kernel

#---- link library
library ITDetLib ../src/Lib/*.cpp 

#---- component library
library ITDet ../src/Component/*.cpp

# =========== standard patterns (the order is essential!) ======================
apply_pattern component_library library=ITDet
apply_pattern linker_library    library=ITDetLib
apply_pattern library_stamps    library=ITDetLib










