package STDet
version v1r2

branches cmt doc src STDet

include_dirs $(STDETROOT)

use DetDescCnv	v2r*	Det
use LHCbKernel	v4r*	Kernel
use GSL		v*	-no_auto_imports

#---- link library
library STDetLib ../src/Lib/*.cpp -import=GSL

#---- component library
library STDet ../src/Component/*.cpp

# =========== standard patterns (the order is essential!) ======================
apply_pattern component_library library=STDet
apply_pattern linker_library    library=STDetLib
