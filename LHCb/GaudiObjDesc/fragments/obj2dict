$(dest)${NAME}.obj2dict :  ${FULLNAME}
	@echo Producing Dictionary from ${FULLNAME}
	@if !(test -d $(dest)); then mkdir $(dest); fi 
	@cd $(dest); $(GAUDIOBJDESCROOT)/$(tag)/$(headertool) -d $(GODflags) -x $(GAUDIOBJDESCROOT)/xml_files/ ${FULLNAME}
	@cd $(dest); $(GAUDIOBJDESCROOT)/$(tag)/$(dicttool) $(GODflags) $(${CONSTITUENT}_GODflags) -x $(GAUDIOBJDESCROOT)/xml_files/ ${FULLNAME}
	@echo /dev/null > $@
