$(dest)${NAME}.obj2dict :  ${FULLNAME}
	@echo Producing Dictionary files from ${FULLNAME}
	@if !(test -d $(dest)); then mkdir $(dest); fi 
	@export LD_LIBRARY_PATH=$(Python_home)/lib; cd $(dest); $(pythonexe) $(parsetool) $(GODflags) $(${CONSTITUENT}_GODflags) -g dct -r $(GAUDIOBJDESCROOT) ${FULLNAME}
	@echo /dev/null > $@
