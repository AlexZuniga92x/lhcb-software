// check for TDR and MC truth
ApplicationMgr.TopAlg += { "CheckSelResult/B2HHAndTDR" };
B2HHAndTDR.ANDmode = true ;
B2HHAndTDR.Algorithms = { "TDRselB2HH" , "AllB2HHTracks" } ;
B2HHAndTDR.AvoidSelResult = false ;

// correlation of HLT and TDR
ApplicationMgr.TopAlg += { "SelResultCorrelations/CorrB2HH" };
CorrB2HH.Algorithms = { "AllB2HHTracks", 
			"HLTFilterB2HH", 
			"TDRselBd2PiPi",
			"TDRselBd2KPi",
			"TDRselBs2PiK",
			"TDRselBs2KK",
			"TDRselB2HH",
			"B2HHAndTDR" } ;

