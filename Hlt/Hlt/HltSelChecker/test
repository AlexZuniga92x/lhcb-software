// Selection Filter configuration
ApplicationMgr.TopAlg +={ "Sequencer/SeqTDRselBd2MuMuKstar" };
SeqTDRselBd2MuMuKstar.Members = {
//  "PreLoadParticles/Combined",
  "MuMu/TDRselMuMu",
  "Kstar2KPiForBd2MuMuKstar/TDRselKstar2KPiForBd2MuMuKstar",
  "Bd2MuMuKstar/TDRselBd2MuMuKstar",
  "PrintHeader/PrintBd2MuMuKstar"
};

Combined.PhysDesktop.ParticleMakerType = "CombinedParticleMaker";
// --> Including file $BD2MUMUKSTARROOT/options/TDRselBd2MuMuKstar.opts


/** @file 
 *  TDR-selection
 *  Recommended options for MuMu, Kstar2KPiForBd2MuMuKstar and Bd2MuMuKstar 
 * algorithms for selection of mu+mu- pairs, K*0(Kpi) and Bd for 
 * Bd -> mu+mu- K*0 channel
 *
 *  @author H.Lopes
 *  @date 2004-04-07
 */

TDRselMuMu.HistogramFlag = false;

//-----------------------------------------------------------------
// Configure PhysDesktop Tool with necessary values or values
// different from default
//-----------------------------------------------------------------
TDRselMuMu.PhysDesktop.InputLocations = {"/Event/Phys/Combined"};

//************************************************************************

TDRselKstar2KPiForBd2MuMuKstar.HistogramFlag = false;

TDRselKstar2KPiForBd2MuMuKstar.PhysDesktop.InputLocations = {"/Event/Phys/Combined"};

//************************************************************************

// Save one or many B0's per event? (lose X final analysis)
TDRselBd2MuMuKstar.oneB0perEvt = true; // Default is false

TDRselBd2MuMuKstar.HistogramFlag = false;

TDRselBd2MuMuKstar.PhysDesktop.InputLocations = {"/Event/Phys/Combined",
					   "/Event/Phys/TDRselMuMu", 
                                           "/Event/Phys/TDRselKstar2KPiForBd2MuMuKstar" };

TDRselBd2MuMuKstar.DecayDescriptor = "{ B0 ->  K+ pi- mu- mu+ , B~0 -> K- pi+ mu+ mu- }";

//**************************************************************

// <-- End of including file $BD2MUMUKSTARROOT/options/TDRselBd2MuMuKstar.opts

// --> Including file $BD2MUMUKSTARROOT/options/TDRselCuts.opts


// Final cuts as on June, 2003.(DaVinci v8r1)
//MuMu:
TDRselMuMu.MuPtMin=900.0;  // MeV
TDRselMuMu.MuMuMassMax=5200.0;
TDRselMuMu.MuMuCdaMax = 1000.1; // dummy
TDRselMuMu.MuMuVtxChi2Max = 8.0;
TDRselMuMu.JPsiMassMax = 3200.0;
TDRselMuMu.JPsiMassMin = 2900.0;
TDRselMuMu.Psi2sMassMax = 3725.0; 
TDRselMuMu.Psi2sMassMin = 3650.0;

//Kstar:
TDRselKstar2KPiForBd2MuMuKstar.PiPtMin = 200.0;  // MeV
TDRselKstar2KPiForBd2MuMuKstar.KPiCdaMax = 10000.0 ; // mm, dummy
TDRselKstar2KPiForBd2MuMuKstar.KPiAngMax = 3.14; // rad
TDRselKstar2KPiForBd2MuMuKstar.KstarMassWindow = 100.0; // MeV
TDRselKstar2KPiForBd2MuMuKstar.KstarZWindow = 10000.0;// mm, dummy
TDRselKstar2KPiForBd2MuMuKstar.Chi2UncFitMax = 8.0;
TDRselKstar2KPiForBd2MuMuKstar.PMin = 0.0; // MeV
TDRselKstar2KPiForBd2MuMuKstar.PtMin = 900.0; // MeV
TDRselKstar2KPiForBd2MuMuKstar.ZVertexMax = 150000.0;  // dummy

// B0:
TDRselBd2MuMuKstar.ImpParamKstarMuMuMax = 10000.0; 
TDRselBd2MuMuKstar.Chi2UncFitMax = 2.0;  // This is chi2/nDoF, nDoF=5 for 4 tracks unconstr. fit
TDRselBd2MuMuKstar.B0ZWindow = 5000.0; // dummy
TDRselBd2MuMuKstar.ImpParamB0Max =3.5;
TDRselBd2MuMuKstar.MuIpMin = 0; // dummy
TDRselBd2MuMuKstar.MuMu2PVDistMin=1.5;
TDRselBd2MuMuKstar.Kstar2PVDistMin=1.5;
TDRselBd2MuMuKstar.ipKstarPiMin=2.0;
TDRselBd2MuMuKstar.ipKstarKMin=2.0;
TDRselBd2MuMuKstar.B02PVDistMin = 0.0; // dummy
TDRselBd2MuMuKstar.ipClose2B0Cut=3.0;
TDRselBd2MuMuKstar.NClose2B0Max=11;
//TDRselBd2MuMuKstar.B0MassWindow = 50.0;
TDRselBd2MuMuKstar.B0MassWindow = 1000.0;

// <-- End of including file $BD2MUMUKSTARROOT/options/TDRselCuts.opts


