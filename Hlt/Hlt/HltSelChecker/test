 *  @date 2003-03-18
 */

//-----------------------------------------------------------------
// Configure PhysDeskTop Tool with necessary values or values
// different from default
//-----------------------------------------------------------------

Jpsi2MuMuForTDRselBs2JpsiPhi.PhysDesktop.InputLocations = {"/Event/Phys/CombinedForTDRselBs2Jpsi2MuMu_Phi2KK"};
Jpsi2MuMuForTDRselBs2JpsiPhi.DecayDescriptor = "J/psi(1S) -> mu+ mu-";
Jpsi2MuMuForTDRselBs2JpsiPhi.Selections = { "J/psi(1S) : BooleanFilterCriterion/JpsiFilter",
                                            "      mu+ : BooleanFilterCriterion/LooseMuonFilter",
                                            "      mu- : BooleanFilterCriterion/LooseMuonFilter"
                                          };
// Jpsi2MuMuForTDRselBs2JpsiPhi.MassWindowLoose = 200; // MeV


Jpsi2MuMuForTDRselBs2JpsiPhi.LooseMuonFilter.AndList = { "KinFilterCriterion" };
Jpsi2MuMuForTDRselBs2JpsiPhi.LooseMuonFilter.KinFilterCriterion.MinMomentum = 0 ; //MeV
Jpsi2MuMuForTDRselBs2JpsiPhi.LooseMuonFilter.KinFilterCriterion.MinPt       = 500 ; //MeV

Jpsi2MuMuForTDRselBs2JpsiPhi.JpsiFilter.AndList = { "KinFilterCriterion","MassFilterCriterion" };
Jpsi2MuMuForTDRselBs2JpsiPhi.JpsiFilter.KinFilterCriterion.MinMomentum =    0; // MeV
Jpsi2MuMuForTDRselBs2JpsiPhi.JpsiFilter.KinFilterCriterion.MinPt       = 0; // MeV
Jpsi2MuMuForTDRselBs2JpsiPhi.JpsiFilter.MassFilterCriterion.Window     = 200; // MeV

