ApplicationMgr.TopAlg += { "PreLoadParticles/Combined" } ;
// --> Including file $B2HHROOT/options/DoTDRselB2HH.opts


/** @file 
 *  Configuration options for the execution of the TDR-selection of
 *  for all B-> HH selections 
 *  Useful for HLT studies
 *
 *  @author P. Koppenburg
 *  @date 2004-10-07
 */

// Necessary DLL
ApplicationMgr.DLLs  += { "B2HH" };

ApplicationMgr.TopAlg += {
//  "PreLoadParticles/Combined",
  "B2hh/TDRselBd2PiPi",
  "B2hh/TDRselBd2KPi",
  "B2hh/TDRselBs2KK",
  "B2hh/TDRselBs2PiK"
};

// --> Including file $B2HHROOT/options/TDRselBd2PiPi.opts


/** @file 
 *  Recommended options for Bd2PiPi algorithm for selection of
 *  Bd -> pi+ pi- with settings used for Re-optimization TDR
 *
 *  @author V.Vagnoni
 *  @date 2004-04-22
 */

/////////////////////////////////////////////////////////////////////////
// Job options for Bd->pi+pi- selection
/////////////////////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////////////////////
TDRselBd2PiPi.OutputLevel = 4;
TDRselBd2PiPi.PhysDesktop.OutputLevel = 4;
/////////////////////////////////////////////////////////////////////////
// Particle names
TDRselBd2PiPi.BMesonName = "B0";
TDRselBd2PiPi.positiveParticleName = "pi+";
TDRselBd2PiPi.negativeParticleName = "pi-";
/////////////////////////////////////////////////////////////////////////
// invariant mass cuts
TDRselBd2PiPi.deltaMassCut = 600.0; // MeV/c2

// max chi2 value of secondary vertex fit
TDRselBd2PiPi.chi2VertexFitCut = 4.0;

// min accepted pt of smallest pt of the pair
TDRselBd2PiPi.ptSmallestCut = 1200.0; // MeV/c

// min accepted pt of highest pt of the pair
TDRselBd2PiPi.ptLargestCut = 3200.0; // MeV/c

// min accepted pt of the B meson candidate
TDRselBd2PiPi.ptBMesonCut = 1600.0; // MeV/c

// min impact parameter significance accepted for the smallest one of the pair
TDRselBd2PiPi.ipSignificanceSmallestCut = 6.0;

// min impact parameter significance accepted of the largest one of the pair
TDRselBd2PiPi.ipSignificanceLargestCut = 12.0;

// max accepted impact parameter significance of the candidate B meson
TDRselBd2PiPi.ipSignificanceBMesonCut = 2.25;

// min accepted distance of flight significance of the B meson candidate
TDRselBd2PiPi.dofSignificanceBMesonCut = 19.0;

// min accepted momentum of the pions
TDRselBd2PiPi.minMomentumCut = 2500.0; // MeV/c
 
// max accepted momentum of the pions
TDRselBd2PiPi.maxMomentumCut = 100000.0; // MeV/c  
/////////////////////////////////////////////////////////////////////////
Combined.PhysDesktop.ParticleMakerType = "CombinedParticleMaker";
/////////////////////////////////////////////////////////////////////////
TDRselBd2PiPi.PhysDesktop.InputLocations = {"/Event/Phys/Combined"};
/////////////////////////////////////////////////////////////////////////
TDRselBd2PiPi.DecayDescriptor = "B0 -> pi+ pi-";

// <-- End of including file $B2HHROOT/options/TDRselBd2PiPi.opts

#include "$B2HHROOT/options/TDRselBd2PiPi.opts"
// --> Including file $B2HHROOT/options/TDRselBd2KPi.opts


/** @file 
 *  Recommended options for Bd2KPi algorithm for selection of
 *  Bd -> K pi with settings used for Re-optimization TDR
 *
 *  @author V.Vagnoni
 *  @date 2004-04-22
 */

/////////////////////////////////////////////////////////////////////////
// Job options for Bd->Kpi selection
/////////////////////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////////////////////
TDRselBd2KPi.OutputLevel = 4;
TDRselBd2KPi.PhysDesktop.OutputLevel = 4;
/////////////////////////////////////////////////////////////////////////
// Particle names
TDRselBd2KPi.BMesonName = "B0";
TDRselBd2KPi.positiveParticleName = "K+";
TDRselBd2KPi.negativeParticleName = "pi-";
/////////////////////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////////////////////
// invariant mass cuts
TDRselBd2KPi.deltaMassCut = 600.0; // MeV/c2

// max chi2 value of secondary vertex fit
TDRselBd2KPi.chi2VertexFitCut = 5.0;

// min accepted pt of smallest pt of the pair
TDRselBd2KPi.ptSmallestCut = 1200.0; // MeV/c

// min accepted pt of highest pt of the pair
TDRselBd2KPi.ptLargestCut = 3000.0; // MeV/c

// min accepted pt of the B meson candidate
TDRselBd2KPi.ptBMesonCut = 1400.0; // MeV/c

// min impact parameter significance accepted for the smallest one of the pair
TDRselBd2KPi.ipSignificanceSmallestCut = 6.0;

// min impact parameter significance accepted of the largest one of the pair
TDRselBd2KPi.ipSignificanceLargestCut = 11.0;

// max accepted impact parameter significance of the candidate B meson
TDRselBd2KPi.ipSignificanceBMesonCut = 2.5;

// min accepted distance of flight significance of the B meson candidate
TDRselBd2KPi.dofSignificanceBMesonCut = 17.0;

// min accepted momentum of the pions
TDRselBd2KPi.minMomentumCut = 2750.0; // MeV/c
 
// max accepted momentum of the pions
TDRselBd2KPi.maxMomentumCut = 200000.0; // MeV/c 
/////////////////////////////////////////////////////////////////////////
TDRselBd2KPi.PhysDesktop.InputLocations = {"/Event/Phys/Combined"};
/////////////////////////////////////////////////////////////////////////
Combined.PhysDesktop.ParticleMakerType = "CombinedParticleMaker";
/////////////////////////////////////////////////////////////////////////
TDRselBd2KPi.DecayDescriptor = "[B0]nos -> K+ pi- , [B0]os -> K- pi+";

// <-- End of including file $B2HHROOT/options/TDRselBd2KPi.opts

#include "$B2HHROOT/options/TDRselBd2KPi.opts"
// --> Including file $B2HHROOT/options/TDRselBs2KK.opts


/** @file 
 *  Recommended options for Bs2KK algorithm for selection of
 *  Bs -> K+ K- with settings used for Re-optimization TDR
 *
 *  @author V.Vagnoni
 *  @date 2004-04-22
 */

/////////////////////////////////////////////////////////////////////////
// Job options for Bs->K+K- selection
/////////////////////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////////////////////
TDRselBs2KK.OutputLevel = 4;
TDRselBs2KK.PhysDesktop.OutputLevel = 4;
/////////////////////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////////////////////
// Particle names
TDRselBs2KK.BMesonName = "B_s0";
TDRselBs2KK.positiveParticleName = "K+";
TDRselBs2KK.negativeParticleName = "K-";
/////////////////////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////////////////////
// invariant mass cuts
TDRselBs2KK.deltaMassCut = 600.0; // MeV/c2

// max chi2 value of secondary vertex fit
TDRselBs2KK.chi2VertexFitCut = 5.0;

// min accepted pt of smallest pt of the pair
TDRselBs2KK.ptSmallestCut = 800.0; // MeV/c

// min accepted pt of highest pt of the pair
TDRselBs2KK.ptLargestCut = 2600.0; // MeV/c

// min accepted pt of the B meson candidate
TDRselBs2KK.ptBMesonCut = 1000.0; // MeV/c

// min impact parameter significance accepted for the smallest one of the pair
TDRselBs2KK.ipSignificanceSmallestCut = 5.0;

// min impact parameter significance accepted of the largest one of the pair
TDRselBs2KK.ipSignificanceLargestCut = 9.0;

// max accepted impact parameter significance of the candidate B meson
TDRselBs2KK.ipSignificanceBMesonCut = 2.75;

// min accepted distance of flight significance of the B meson candidate
TDRselBs2KK.dofSignificanceBMesonCut = 14.0;

// min accepted momentum of the pions
TDRselBs2KK.minMomentumCut = 2750.0; // MeV/c

// max accepted momentum of the pions
TDRselBs2KK.maxMomentumCut = 125000.0; // MeV/c
/////////////////////////////////////////////////////////////////////////
TDRselBs2KK.PhysDesktop.InputLocations = {"/Event/Phys/Combined"};
/////////////////////////////////////////////////////////////////////////
Combined.PhysDesktop.ParticleMakerType = "CombinedParticleMaker";
/////////////////////////////////////////////////////////////////////////
TDRselBs2KK.DecayDescriptor = "B_s0 -> K+ K-";

// <-- End of including file $B2HHROOT/options/TDRselBs2KK.opts

#include "$B2HHROOT/options/TDRselBs2KK.opts"
// --> Including file $B2HHROOT/options/TDRselBs2PiK.opts


/** @file 
 *  Recommended options for Bs2PiK algorithm for selection of
 *  Bs -> pi K with settings used for Re-optimization TDR
 *
 *  @author V.Vagnoni
 *  @date 2004-04-22
 */

/////////////////////////////////////////////////////////////////////////
// Job options for Bs->piK selection
/////////////////////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////////////////////
TDRselBs2PiK.OutputLevel = 4;
TDRselBs2PiK.PhysDesktop.OutputLevel = 4;
/////////////////////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////////////////////
// Particle names
TDRselBs2PiK.BMesonName = "B_s0";
TDRselBs2PiK.positiveParticleName = "K+";
TDRselBs2PiK.negativeParticleName = "pi-";
/////////////////////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////////////////////
// invariant mass cuts
TDRselBs2PiK.deltaMassCut = 1200.0; // MeV/c2

// max chi2 value of secondary vertex fit
TDRselBs2PiK.chi2VertexFitCut = 4.0;

// min accepted pt of smallest pt of the pair
TDRselBs2PiK.ptSmallestCut = 1400.0; // MeV/c

// min accepted pt of highest pt of the pair
TDRselBs2PiK.ptLargestCut = 3400.0; // MeV/c

// min accepted pt of the B meson candidate
TDRselBs2PiK.ptBMesonCut = 1600.0; // MeV/c

// min impact parameter significance accepted for the smallest one of the pair
TDRselBs2PiK.ipSignificanceSmallestCut = 7.0;

// min impact parameter significance accepted of the largest one of the pair
TDRselBs2PiK.ipSignificanceLargestCut = 14.0;

// max accepted impact parameter significance of the candidate B meson
TDRselBs2PiK.ipSignificanceBMesonCut = 2.25;

// min accepted distance of flight significance of the B meson candidate
TDRselBs2PiK.dofSignificanceBMesonCut = 20.0;

// min accepted momentum of the pions
TDRselBs2PiK.minMomentumCut = 2750.0; // MeV/c

// max accepted momentum of the pions
TDRselBs2PiK.maxMomentumCut = 100000.0; // MeV/c
/////////////////////////////////////////////////////////////////////////
TDRselBs2PiK.PhysDesktop.InputLocations = {"/Event/Phys/Combined"};
/////////////////////////////////////////////////////////////////////////
Combined.PhysDesktop.ParticleMakerType = "CombinedParticleMaker";
/////////////////////////////////////////////////////////////////////////
TDRselBs2PiK.DecayDescriptor = "[B_s0]nos -> K- pi+, [B_s0]os -> K+ pi-";

// <-- End of including file $B2HHROOT/options/TDRselBs2PiK.opts

#include "$B2HHROOT/options/TDRselBs2PiK.opts"

// merge B->HH selresults
ApplicationMgr.TopAlg += { "CheckSelResult/TDRselB2HH" };
TDRselB2HH.ANDmode = false ; // OR
TDRselB2HH.Algorithms = { "TDRselBd2PiPi",
			   "TDRselBd2KPi",
			   "TDRselBs2KK",
			   "TDRselBs2PiK" } ;
TDRselB2HH.AvoidSelResult = false ;
TDRselB2HH.OutputLevel = 4 ;

// <-- End of including file $B2HHROOT/options/DoTDRselB2HH.opts

#include "$B2HHROOT/options/DoTDRselB2HH.opts" 
