/** @file 
 *  Options for the preselection of Bs->DsH
 *
 *  @author Jeroen van Tilburg
 *  @date 2003-02-17
 */

// Charged particle options
CombinedForBs2DsH.PhysDesktop.ParticleMakerType =  "CombinedParticleMaker";
CombinedForBs2DsH.PhysDesktop.CombinedParticleMaker.ExclusiveSelection = false ;
CombinedForBs2DsH.PhysDesktop.CombinedParticleMaker.Particles          = 
  { "kaon" , "pion" };
CombinedForBs2DsH.PhysDesktop.CombinedParticleMaker.KaonSelection      = 
  {"det='RICH' k-pi='-5.0' k-e='-5.0' k-p='-5.0'" } ;
CombinedForBs2DsH.PhysDesktop.CombinedParticleMaker.PionSelection      = 
  {"det='RICH' pi-k='-5.0'"           } ;

// Selection algorithm default options 
TDRselBs2DsK.PhysDesktop.InputLocations = { "/Event/Phys/CombinedForBs2DsH" } ;
TDRselBs2DsK.UseMCTruth     = false      ; // MC Associators are not loaded
TDRselBs2DsK.Cuts           = 4          ; // 1=strip, 4=Final(=TDR)
TDRselBs2DsK.DsName         = "Ds"       ; // Only Ds search
TDRselBs2DsK.bachelorName   = "K"        ; // select only bachelor kaons
TDRselBs2DsK.DecayDescriptor = "{ [B_s0]nos -> (D_s- => K- K+ pi-) K+, [B_s0]os  -> (D_s+ => K- K+ pi+) K-  }";
TDRselBs2DsK.VertexFit.CombinedTransporter.OutputLevel = 5;


// Due to bug in LoKi
TDRselBs2DsK.P2PVsTable = "";

// <-- End of including file $BS2DSHROOT/options/TDRselBs2DsK.opts

// --> Including file $BS2DSHROOT/options/TDRselBs2DsPi.opts


