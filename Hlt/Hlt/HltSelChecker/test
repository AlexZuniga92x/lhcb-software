// --> Including file $BS2JPSIPHIROOT/options/Phi2KKForTDRselBs2JpsiPhi.opts


/** @file 
 *  Recommended options for Phi2KKForTDRselBs2JpsiPhi algorithm 
 *  (an instance of CombineParticles) for selection of
 *  phi -> K+K- for Bd -> J/Psi phi
 *
 *  @author G. Raven
 *  @date 2003-03-18
 */

Phi2KKForTDRselBs2JpsiPhi.DecayDescriptor = "phi(1020) -> K+ K-";
//-----------------------------------------------------------------
// Configure PhysDeskTop Tool with necessary values or values
// different from default
//-----------------------------------------------------------------
Phi2KKForTDRselBs2JpsiPhi.PhysDesktop.InputLocations = {"/Event/Phys/CombinedForTDRselBs2Jpsi2MuMu_Phi2KK"};

//-----------------------------------------------------------------
// Configure which filters to apply to which  particles
//-----------------------------------------------------------------
Phi2KKForTDRselBs2JpsiPhi.Selections = {"       K+ : BooleanFilterCriterion/KaonFilter",
                                        "       K- : BooleanFilterCriterion/KaonFilter",
                                        "phi(1020) : BooleanFilterCriterion/PhiFilter" };

Phi2KKForTDRselBs2JpsiPhi.KaonFilter.AndList = { "KinFilterCriterion"  };
Phi2KKForTDRselBs2JpsiPhi.KaonFilter.KinFilterCriterion.MinPt = 500 ; //MeV
Phi2KKForTDRselBs2JpsiPhi.PhiFilter.AndList = { "KinFilterCriterion",
                                                "VtxFilterCriterion",
                                                "MassFilterCriterion" };
Phi2KKForTDRselBs2JpsiPhi.PhiFilter.KinFilterCriterion.MinMomentum = 12000; // MeV
Phi2KKForTDRselBs2JpsiPhi.PhiFilter.KinFilterCriterion.MinPt       =     0; // MeV
Phi2KKForTDRselBs2JpsiPhi.PhiFilter.VtxFilterCriterion.MaxChi2     =    40; 
Phi2KKForTDRselBs2JpsiPhi.PhiFilter.VtxFilterCriterion.MinZPos     =  -500; // mm
Phi2KKForTDRselBs2JpsiPhi.PhiFilter.VtxFilterCriterion.MaxZPos     =   500; // mm
Phi2KKForTDRselBs2JpsiPhi.PhiFilter.MassFilterCriterion.Window     =    20; // MeV


//-----------------------------------------------------------------
// Configure the
// mass window the daughters should pass _before_ we make a
// mother candidate
//-----------------------------------------------------------------
// Phi2KKForTDRselBs2JpsiPhi.MassWindowLoose = 220; // MeV

//**************************************************************

// <-- End of including file $BS2JPSIPHIROOT/options/Phi2KKForTDRselBs2JpsiPhi.opts

// --> Including file $BS2JPSIPHIROOT/options/TDRselBs2Jpsi2MuMu_Phi2KK.opts


/** @file 
 *  Recommended options for TDRSelBs2Jpsi2MuMu_Phi2KK algorithm 
 *  (an instance of CombineParticles) for selection of
 *  Bd -> J/Psi (mu+mu-) phi (K+K-)
 *
 *  @author G. Raven
 *  @date 2003-03-18
 */

TDRSelBs2Jpsi2MuMu_Phi2KK.PhysDesktop.InputLocations = { "/Event/Phys/Jpsi2MuMuForTDRselBs2JpsiPhi",
                                                         "/Event/Phys/Phi2KKForTDRselBs2JpsiPhi"};

TDRSelBs2Jpsi2MuMu_Phi2KK.DecayDescriptor = "B_s0 -> J/psi(1S) phi(1020) ";
// TDRSelBs2Jpsi2MuMu_Phi2KK.MassWindowLoose = 800; // MeV

TDRSelBs2Jpsi2MuMu_Phi2KK.Selections = {" J/psi(1S) : BooleanFilterCriterion/JpsiFilter", 
                                        " phi(1020) : BooleanFilterCriterion/PhiFilter",
                                        "      B_s0 : BooleanFilterCriterion/BsFilter" };

TDRSelBs2Jpsi2MuMu_Phi2KK.JpsiFilter.AndList = {"MassFilterCriterion",
                                                "VtxFilterCriterion" };
TDRSelBs2Jpsi2MuMu_Phi2KK.PhiFilter.AndList = { "MassFilterCriterion",
                                                "KinFilterCriterion" };
TDRSelBs2Jpsi2MuMu_Phi2KK.BsFilter.AndList = { "VtxFilterCriterion",
                                               "MassFilterCriterion",
                                               "LifetimeSignificanceCriterion" };

TDRSelBs2Jpsi2MuMu_Phi2KK.JpsiFilter.MassFilterCriterion.Window    = 50; // MeV
TDRSelBs2Jpsi2MuMu_Phi2KK.JpsiFilter.VtxFilterCriterion.MaxChi2    = 9;

TDRSelBs2Jpsi2MuMu_Phi2KK.PhiFilter.MassFilterCriterion.Window     =     20; // MeV
TDRSelBs2Jpsi2MuMu_Phi2KK.PhiFilter.KinFilterCriterion.MinMomentum =  12000; // MeV

TDRSelBs2Jpsi2MuMu_Phi2KK.BsFilter.VtxFilterCriterion.MaxChi2      =   20;
TDRSelBs2Jpsi2MuMu_Phi2KK.BsFilter.VtxFilterCriterion.MinZPos      = -500; // mm
TDRSelBs2Jpsi2MuMu_Phi2KK.BsFilter.VtxFilterCriterion.MaxZPos      =  500; // mm
TDRSelBs2Jpsi2MuMu_Phi2KK.BsFilter.MassFilterCriterion.Window      =  600; // MeV
TDRSelBs2Jpsi2MuMu_Phi2KK.BsFilter.LifetimeSignificanceCriterion.MinSignificance =     5;
TDRSelBs2Jpsi2MuMu_Phi2KK.BsFilter.LifetimeSignificanceCriterion.MaxChisq        =   100;
TDRSelBs2Jpsi2MuMu_Phi2KK.BsFilter.LifetimeSignificanceCriterion.UseAssociator   = false;

//**************************************************************

// <-- End of including file $BS2JPSIPHIROOT/options/TDRselBs2Jpsi2MuMu_Phi2KK.opts


TDRSelBs2Jpsi2MuMu_Phi2KK.setFilter = true;
PrintBs2Jpsi2MuMu_Phi2KK.OutputLevel = 2;

