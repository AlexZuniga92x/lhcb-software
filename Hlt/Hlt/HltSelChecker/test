// Configure its PIDFilterCriterion Tool
//-----------------------------------------------------------------
Ks2PiPiForTDRselBd2JpsiKsTTT.ParticleFilter.PIDFilterCriterion.ParticleNames 
= { "pi-", "pi+" };
Ks2PiPiForTDRselBd2JpsiKsTTT.ParticleFilter.PIDFilterCriterion.ConfidenceLevels 
= { 0.0, 0.0 };

Ks2PiPiForTDRselBd2JpsiKsTTT.UnconstVertexFitter.Transporter="TrTransporter";
Ks2PiPiForTDRselBd2JpsiKsTTT.ParticleStuffer.Transporter="TrTransporter";
Ks2PiPiForTDRselBd2JpsiKsTTT.LagrangeMassVertexFitter.UnconstVertexFitter.Transporter="TrTransporter";
Ks2PiPiForTDRselBd2JpsiKsTTT.LagrangeMassVertexFitter.Transporter="TrTransporter";

//TrTransporter.Extrapolator = "TrFirstCleverExtrapolator";
TrTransporter.Extrapolator = "TrHerabExtrapolator";
Ks2PiPiForTDRselBd2JpsiKsTTT.ParticleStuffer.TrTransporter.Extrapolator = "TrHerabExtrapolator";






// <-- End of including file $BD2JPSIKSROOT/options/Ks2PiPiForTDRselBd2JpsiKsTTT.opts

// --> Including file $BD2JPSIKSROOT/options/TDRselBd2Jpsi2MuMu_Ks2PiPiTTT.opts


/** @file
 *  Recommended options for TDRselBd2Jpsi2MuMuKsTTT algorithm for selection of
 *  Bd -> J/Psi (mu+mu-) Ks (pi+pi-) for UU tracks
 *
 *  @author S.Amato
 *  @date 2003-07-09
 */


TDRselBd2Jpsi2MuMuKsTTT.HistogramFlag = false;
TDRselBd2Jpsi2MuMuKsTTT.B0MassWindowLoose = 600.0;
//tight mass cut
//TDRselBd2Jpsi2MuMuKsTTT.B0MassWindowTight = 60.0;
//loose mass cut
TDRselBd2Jpsi2MuMuKsTTT.B0MassWindowTight = 600.0;
TDRselBd2Jpsi2MuMuKsTTT.ImpParamB0 = 5.0;
TDRselBd2Jpsi2MuMuKsTTT.ImpParamKsJPsi = 8.;
TDRselBd2Jpsi2MuMuKsTTT.MinImpParamPi = 2.0;
TDRselBd2Jpsi2MuMuKsTTT.Chi2UncFitCut = 20.0;
TDRselBd2Jpsi2MuMuKsTTT.MinImpParamMu = 1.2;
TDRselBd2Jpsi2MuMuKsTTT.SigDistanceJpsi = 1.2;
TDRselBd2Jpsi2MuMuKsTTT.SigDistanceKs = -1000.;
TDRselBd2Jpsi2MuMuKsTTT.PtB = 0.;
TDRselBd2Jpsi2MuMuKsTTT.ImpParamKsPV = 0.;
TDRselBd2Jpsi2MuMuKsTTT.KsLocation = "/Event/Phys/Ks2PiPiForTDRselBd2JpsiKsTTT/Particles";
TDRselBd2Jpsi2MuMuKsTTT.JpsiLocation = "/Event/Phys/TDRselJpsi2MuMu/Particles";

TDRselBd2Jpsi2MuMuKsTTT.TThole = 40.0;

TDRselBd2Jpsi2MuMuKsTTT.PhysDesktop.InputLocations = {"/Event/Phys/TDRselJpsi2MuMu",
                                          "/Event/Phys/Ks2PiPiForTDRselBd2JpsiKsTTT",
                                          "/Event/Phys/CombinedForTDRselBd2Jpsi2MuMu_Ks2PiPi_TTT" };

TDRselBd2Jpsi2MuMuKsTTT.DecayDescriptor = "B0 -> (KS0 -> pi+ pi-) (J/psi(1S) -> mu+ mu-)";
TDRselBd2Jpsi2MuMuKsTTT.ParticleFilter.CriteriaNames = { "PIDFilterCriterion" };
TDRselBd2Jpsi2MuMuKsTTT.ParticleFilter.PIDFilterCriterion.ParticleNames = {
  "J/psi(1S)", "KS0" };
TDRselBd2Jpsi2MuMuKsTTT.ParticleFilter.PIDFilterCriterion.ConfidenceLevels = {
  1.0, 1.0 };


// <-- End of including file $BD2JPSIKSROOT/options/TDRselBd2Jpsi2MuMu_Ks2PiPiTTT.opts


TDRselBd2Jpsi2MuMuKsLong.OutputLevel               = 3;
TDRselBd2JpsiKsVTT.OutputLevel              = 3;

TDRselJpsi2mumu.OutputLevel  =3;
TDRselJpsi2mumu.UnconstVertexFitter.OutputLevel  = 5;

TDRselJpsi2mumu.LagrangeMassVertexFitter.OutputLevel  = 4;

Ks2PiPiForTDRselBd2JpsiKsLong.OutputLevel  =3;
Ks2PiPiForTDRselBd2JpsiKsVTT.OutputLevel  =3;

Ks2PiPiForTDRselBd2JpsiKsLong.UnconstVertexFitter.OutputLevel  = 5;
Ks2PiPiForTDRselBd2JpsiKsVTT.UnconstVertexFitter.OutputLevel  = 5;
Ks2PiPiForTDRselBd2JpsiKsVTT.LagrangeMassVertexFitter.OutputLevel  = 5;

Ks2PiPiForTDRselBd2JpsiKsTTT.OutputLevel  =3;
Ks2PiPiForTDRselBd2JpsiKsTTT.UnconstVertexFitter.OutputLevel  = 5;

//PrintTDRselBd2Jpsi2MuMuKsLong.OutputLevel = 2 ;
//PrintTDRselBd2Jpsi2MuMuKsVTT.OutputLevel = 2 ;
//PrintTDRselBd2Jpsi2MuMuKsTTT.OutputLevel = 2 ;
PrintTDRselBd2Jpsi2MuMu_Ks2PiPi.OutputLevel = 2 ;
TDRselBd2Jpsi2MuMu_Ks2PiPi.OutputLevel = 4 ;

CheckTDRselBd2Jpsi2MuMuKsLong.OutputLevel = 5 ;
CheckTDRselBd2Jpsi2MuMuKsVTT.OutputLevel = 5 ;
CheckTDRselBd2Jpsi2MuMuKsTTT.OutputLevel = 5 ;


// <-- End of including file $BD2JPSIKSROOT/options/DoTDRselBd2Jpsi2MuMu_Ks2PiPi.opts


