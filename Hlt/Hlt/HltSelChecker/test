					"NsctVeloTransporter";
TDRselBd2DstarPi.GeomDispCalculator.CombinedTransporter.VeloTransporterName = 
					"NsctVeloTransporter";


//-----------------------------------------------------------------------------
// Configure the Particle Transporter outside the Velo Region 
// Possible options for this version are LinearTransporter or 
// ParabolicTransporter
//-----------------------------------------------------------------------------

TDRselBd2DstarPi.LagrangeMassVertexFitter.CombinedTransporter.MFTransporterName = 
					"LinearTransporter";
TDRselBd2DstarPi.UnconstVertexFitter.CombinedTransporter.MFTransporterName = 
					"LinearTransporter";
TDRselBd2DstarPi.GeomDispCalculator.CombinedTransporter.MFTransporterName = 
					"LinearTransporter";
TDRselBd2DstarPi.LagrangeMassVertexFitter.UnconstVertexFitter.CombinedTransporter.MFTransporterName = 
					"LinearTransporter";



