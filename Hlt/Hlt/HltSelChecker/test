// --> Including file $HLTSELCHECKERROOT/options/TruthCommon.opts


// links
Particle2MCLinks.InputData = { "Phys/HLTPions/Particles" };

ApplicationMgr.DLLs += {"DaVinciMCTools"}; 
TrueSignals.Members += {"PrintMCTree/PrintTrueB"};
PrintTrueB.ParticleNames = {  "B_s0", "B_s~0", "B0", "B~0" } ;
PrintTrueB.DebugTool.Informations = "Name E M P Px Py Pz Pt phi Vz" ;
PrintTrueB.Depth = 3;

TrueSignals.Members += {"PrintMCTree/PrintTrueD"};
PrintTrueD.ParticleNames = {  "D*(2010)-", "D*(2010)+" } ;
PrintTrueD.DebugTool.Informations = "Name E M P Px Py Pz Pt phi Vz" ;
PrintTrueD.Depth = 2;

TrueSignals.Members += {"PrintTree/PrintBTracks"};
PrintBTracks.DebugTool.Informations = "Name E M P Px Py Pz Pt phi Vz" ;
PrintBTracks.PhysDesktop.InputLocations = {, "Phys/AllBd2D02KPiKstarTracks"
                                           , "Phys/AllBd2DstarPiTracks"
                                            ,"Phys/AllBs2DsHTracks"
                                            ,"Phys/AllBs2PhiPhiTracks"
                                           , "Phys/AllB2HHTracks"
					    ,"Phys/AllB2MuMuTracks"
					    ,"Phys/AllBd2JpsiKsTracks"
					   , "Phys/AllBd2MuMuKstarTracks"
					   , "Phys/AllBs2JpsiPhiTracks"
					    ,"Phys/AllBs2PhiPhiTracks"
					     } ;

// <-- End of including file $HLTSELCHECKERROOT/options/TruthCommon.opts

//==============================================================================

// <-- End of including file $HLTSELCHECKERROOT/options/Truth.opts

//==============================================================================
// Overwrite input to take *only* MC truth particles into account
//==============================================================================
// --> Including file $HLTSELCHECKERROOT/options/TruthInput.opts


//==============================================================================
// MC truth again - overwrite input to take only MC truth
//==============================================================================
HLTPreselPions.PhysDesktop.InputLocations = { "Phys/SelectB2HH"
                                              ,"Phys/SelectBs2PhiPhi"
                                              ,"Phys/SelectBd2D02KPiKstar"
                                              ,"Phys/SelectBd2DstarPi"
                                              ,"Phys/SelectBs2DsH"
                                              ,"Phys/SelectBs2PhiGamma" 
//                                              ,"Phys/SelectB2MuMu" 
                                              ,"Phys/SelectBd2MuMuKstar" 
                                              ,"Phys/SelectBs2JpsiPhi" 
//                                              ,"Phys/SelectBd2JpsiKs" 
/                                              ,"Phys/SelectDstar2Dpi" 
					      } ;
HLTKaons.PhysDesktop.InputLocations = {"Phys/SelectBs2DsH"};
HLTMuons.PhysDesktop.InputLocations = {"Phys/SelectBs2JpsiPhi"
                                       ,"Phys/SelectBd2JpsiKs"
                                       ,"Phys/SelectB2MuMu"
				       ,"Phys/SelectBd2MuMuKstar"};


// <-- End of including file $HLTSELCHECKERROOT/options/TruthInput.opts


//==============================================================================
// Prepare particles and intermediate resonances
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTMakeResonances.opts


HLTSelections.Members += {"GaudiSequencer/HLTMakeResonances"};
HLTMakeResonances.IgnoreFilterPassed = true ;
HLTMakeResonances.MeasureTime = true ;

//------------------------------------------------------------------------------
// Apply some pre-selection cuts on pions
HLTMakeResonances.Members += {"RefineSelection/HLTPreselPions"};
HLTPreselPions.PhysDesktop.InputLocations = {"Phys/HLTPions"};
HLTPreselPions.ParticleNames = {"pi+", "pi-"};
HLTPreselPions.FilterNames = {"PiFilter", "PiFilter"};
HLTPreselPions.PiFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
HLTPreselPions.PiFilter.KinFilterCriterion.MinPt = 300.; // MeV
HLTPreselPions.PiFilter.KinFilterCriterion.MinMomentum = 0.; // MeV
HLTPreselPions.PiFilter.PVIPFilterCriterion.MinIPsignif = 0.5; // sigma
//------------------------------------------------------------------------------

//==============================================================================
// Form the seeding vertices, "rho -> pi+ pi-"
//==============================================================================

//------------------------------------------------------------------------------
// Form 2-prong seeding vertices
HLTMakeResonances.Members += {"CombineParticles/HLTSeedingVertices"} ;
HLTSeedingVertices.PhysDesktop.InputLocations = {"Phys/HLTPreselPions"};
HLTSeedingVertices.DecayDescriptor = "rho(770)0 -> pi+ pi-" ;
HLTSeedingVertices.Selections = {   "rho(770)0 : BooleanFilterCriterion/FilterSeeding", 
                                    "pi+ : BooleanFilterCriterion", 
                                    "pi- : BooleanFilterCriterion"} ;
HLTSeedingVertices.FilterSeeding.AndList = { "VtxFilterCriterion",
                                             "KinFilterCriterion", 
                                             "PVIPFilterCriterion" };
HLTSeedingVertices.FilterSeeding.VtxFilterCriterion.MaxChi2 = 200; // hlt tuned: 200, hlt presel: 200 
HLTSeedingVertices.FilterSeeding.KinFilterCriterion.MinPt = 0.; // MeV
HLTSeedingVertices.FilterSeeding.KinFilterCriterion.MinMomentum = 0.; // MeV
HLTSeedingVertices.FilterSeeding.PVIPFilterCriterion.MinIPsignif = 0.; // sigma
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Transform seeding vertices. 
//------------------------------------------------------------------------------
// Transform seeding vertex into a phi -> K+ K-
HLTMakeResonances.Members += {"ChangeDecayChain/HLTSeedingKK"};
HLTSeedingKK.PhysDesktop.InputLocations = {"Phys/HLTSeedingVertices"};
HLTSeedingKK.Decay = "rho(770)0 -> ^pi+ ^pi-";
HLTSeedingKK.Particles = {"rho(770)0", "pi+", "pi-"};
HLTSeedingKK.Replacements = {"phi(1020)", "K+", "K-"};
HLTSeedingKK.SeparateOutputs = true ;
//------------------------------------------------------------------------------
// Transform seeding vertex into a K*(892)0 -> K+ pi- anc cc
HLTMakeResonances.Members += {"ChangeDecayChain/HLTSeedingKPi"};
HLTSeedingKPi.PhysDesktop.InputLocations = {"Phys/HLTSeedingVertices"};
HLTSeedingKPi.Decay = "rho(770)0 -> ^pi+ ^pi-";
HLTSeedingKPi.Particles = {"rho(770)0", "pi+", "pi-"};
HLTSeedingKPi.Replacements = {"K*(892)0", "K+", "pi-"};
HLTSeedingKPi.MoreReplacements = {"K*(892)~0", "pi+", "K-"};
HLTSeedingKPi.SeparateOutputs = true ;
//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
// Now distribute PIDs according to masses
//------------------------------------------------------------------------------
// K* -> Kpi, D_0 -> Kpi
HLTMakeResonances.Members += {"ChangePIDByMass/HLTKPiFrom"};   
HLTKPiFrom.PhysDesktop.InputLocations = {"Phys/HLTSeedingKPiMother"};
HLTKPiFrom.InputParticle = "K*(892)0" ;
HLTKPiFrom.KeepCC = true ;
HLTKPiFrom.OutputParticles = { "K*(892)0", "D~0" } ;
HLTKPiFrom.Windows = { 120.0 , 50.0 } ;
HLTKPiFrom.Locations = { "Kstar", "D0" } ;
//------------------------------------------------------------------------------
// Phi -> KK, D0 -> KK
HLTMakeResonances.Members += {"ChangePIDByMass/HLTKKFrom"};   
HLTKKFrom.PhysDesktop.InputLocations = {"Phys/HLTSeedingKKMother"};
HLTKKFrom.InputParticle = "phi(1020)" ;
HLTKKFrom.KeepCC = true ;
HLTKKFrom.OutputParticles = { "phi(1020)", "D0" } ;
HLTKKFrom.Windows = { 30.0 , 50.0 } ;
HLTKKFrom.Locations = { "Phi", "D0" } ;
//------------------------------------------------------------------------------
// Rho -> pipi, D0->pipi, B -> pipi
HLTMakeResonances.Members += {"ChangePIDByMass/HLTPiPiFrom"};   
HLTPiPiFrom.PhysDesktop.InputLocations = {"Phys/HLTSeedingVertices"};
HLTPiPiFrom.InputParticle = "rho(770)0" ;
HLTPiPiFrom.KeepCC = true ;
HLTPiPiFrom.OutputParticles = { "rho(770)0", "D0", "B0" } ; // rho not used yet
HLTPiPiFrom.Windows = { 500.0 , 50.0, 700.0 } ;
HLTPiPiFrom.Locations = { "Rho", "D0", "B0" } ;

//------------------------------------------------------------------------------
// Shared D0
HLTMakeResonances.Members += {"RefineSelection/HLTSharedD02HH"};   
HLTSharedD02HH.PhysDesktop.InputLocations = {"Phys/HLTKKFromD0", "Phys/HLTKPiFromD0", "Phys/HLTPiPiFromD0"};
HLTSharedD02HH.ParticleNames = {"D~0", "D0", "K+", "pi-", "pi+", "K-"};
HLTSharedD02HH.FilterNames = {"D0Filter", "D0Filter", "Filter", "Filter", "Filter", "Filter"};
HLTSharedD02HH.Filter.CriteriaNames = {"KinFilterCriterion"} ;

HLTSharedD02HH.Filter.KinFilterCriterion.MinPt = 300.; // hlt tuned: 300 MeV, hlt presel: 0 MeV, no offline cut
HLTSharedD02HH.Filter.KinFilterCriterion.MinMomentum = 1000. ; // hlt tuned: 1 GeV, hlt presel: 1 GeV, no offline cut
HLTSharedD02HH.Filter.PVIPFilterCriterion.MinIPsignif = 1.; // hlt tuned: 1, hlt presel: 0, no offline cut
//------------------------------------------------------------------------------
HLTSharedD02HH.D0Filter.CriteriaNames = {"MassFilterCriterion", 
                                           "VtxFilterCriterion"};

HLTSharedD02HH.D0Filter.MassFilterCriterion.Window = 45*MeV ; // hlt tuned: 45 MeV, hlt presel: 200 MeV, offline is 50 MeV (in note) or 30 MeV in TDRsel
HLTSharedD02HH.D0Filter.VtxFilterCriterion.MaxChi2 = 80.; // hlt tuned: 80, hlt presel: 200, offline is 40


// <-- End of including file $HLTSELECTIONSROOT/options/HLTMakeResonances.opts

// --> Including file $HLTSELECTIONSROOT/options/HLTMuonHighway.opts


HLTSelections.Members += {"GaudiSequencer/HLTMuonHighway"};
HLTMuonHighway.IgnoreFilterPassed = true ;  // or dimuons are made only if there are hot muons
HLTMuonHighway.MeasureTime = true ;

//------------------------------------------------------------------------------
// SINGLE MUON
//------------------------------------------------------------------------------
HLTMuonHighway.Members += {"RefineSelection/HLTHotMuons"};
HLTHotMuons.PhysDesktop.InputLocations = {"Phys/HLTMuons"};
HLTHotMuons.ParticleNames = {"mu+", "mu-"};
HLTHotMuons.FilterNames = {"MuFilter", "MuFilter"};
HLTHotMuons.MuFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
HLTHotMuons.MuFilter.KinFilterCriterion.MinPt = 2000.*MeV ; // MeV
HLTHotMuons.MuFilter.KinFilterCriterion.MinMomentum = 0.; // MeV
HLTHotMuons.MuFilter.PVIPFilterCriterion.MinIPsignif = 3.; // sigma
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// DIMUONS - to be re-used in K*MuMu for instance
//------------------------------------------------------------------------------
HLTMuonHighway.Members += {"CombineParticles/HLTDimuons"} ;
HLTDimuons.PhysDesktop.InputLocations = {"Phys/HLTMuons"};
HLTDimuons.DecayDescriptor = "J/psi(1S) -> mu+ mu-" ;
HLTDimuons.Selections = {  "J/psi(1S) : BooleanFilterCriterion/DimuFilter", 
                           "mu+ : BooleanFilterCriterion/MuFilter", 
                           "mu- : BooleanFilterCriterion/MuFilter"} ;
HLTDimuons.DimuFilter.AndList = { "VtxFilterCriterion"};
HLTDimuons.DimuFilter.VtxFilterCriterion.MaxChi2 = 200.; // 

// Need probably to refine Vertex separation and IP for MuMuK*
// To be done in the HLTselMuMuKstar.opts file
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// J/Psi Candidates, a subset of the dimuons
//------------------------------------------------------------------------------
HLTMuonHighway.Members += {"RefineSelection/HLTAllJpsis"} ;
HLTAllJpsis.PhysDesktop.InputLocations = { "Phys/HLTDimuons" };
HLTAllJpsis.ParticleNames = { "J/psi(1S)", "mu+", "mu-"};
HLTAllJpsis.FilterNames = {"PsiFilter","MuFilter","MuFilter" };

HLTAllJpsis.PsiFilter.CriteriaNames = {  "MassFilterCriterion" };
HLTAllJpsis.PsiFilter.MassFilterCriterion.LowerWindow = 500.*MeV ; // below the Psi
HLTAllJpsis.PsiFilter.MassFilterCriterion.UpperWindow = 100.*GeV ; // Up to Z->MuMu :-)

HLTAllJpsis.MuFilter.CriteriaNames = {  "KinFilterCriterion" };
HLTAllJpsis.MuFilter.KinFilterCriterion.MinPt = 0. ; // cut again on Pt?

//------------------------------------------------------------------------------
// Hot J/Psi Candidates, a subset of the Jpsis
//------------------------------------------------------------------------------
HLTMuonHighway.Members += {"RefineSelection/HLTHotJpsis"} ;
HLTHotJpsis.PhysDesktop.InputLocations = { "Phys/HLTAllJpsis" };
HLTHotJpsis.ParticleNames = { "J/psi(1S)", "mu+", "mu-"};
HLTHotJpsis.FilterNames = {"PsiFilter","MuFilter","MuFilter" };

HLTHotJpsis.PsiFilter.CriteriaNames = {  "MassFilterCriterion","FlightDistanceFilterCriterion" };
HLTHotJpsis.PsiFilter.MassFilterCriterion.LowerWindow = 300.*MeV ; // below the Psi
HLTHotJpsis.PsiFilter.MassFilterCriterion.UpperWindow = 300.*MeV ; // Up to Z->MuMu :-)
HLTHotJpsis.PsiFilter.FlightDistanceFilterCriterion.MinFSPV = 7.;

HLTHotJpsis.MuFilter.CriteriaNames = {  "KinFilterCriterion", "PVIPFilterCriterion" };
HLTHotJpsis.MuFilter.KinFilterCriterion.MinPt = 0.3 ; // cut again on Pt
HLTHotJpsis.MuFilter.PVIPFilterCriterion.MinIPsignif = 2.;

// <-- End of including file $HLTSELECTIONSROOT/options/HLTMuonHighway.opts

//==============================================================================
// HLT Selection for generic D* stream
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselDstar.opts


//==============================================================================
// HLT Selection of D*(2010)- -> (D~0 -> h+ h-)  pi-) pi+ 
//==============================================================================
// Author: P. Koppenburg   12/11/2004

HLTSelections.Members += {"GaudiSequencer/HLTselDstar"};
HLTselDstar.IgnoreFilterPassed = false ;
HLTselDstar.MeasureTime = true ;

//------------------------------------------------------------------------------
// HLTDstar2D02HH  !! Take care of wrong-charge too
// Might be used for Bd->Dstar pi too?
HLTselDstar.Members += {"CombineParticles/HLTDstar2D02HH"};
HLTDstar2D02HH.PhysDesktop.InputLocations = {"Phys/HLTSharedD02HH", "Phys/HLTPions"}; // no preselection!
HLTDstar2D02HH.DecayDescriptor = " [ D*(2010)- -> D~0 pi- ]cc ";
HLTDstar2D02HH.Selections = {"D*(2010)- : BooleanFilterCriterion/DstarFilter",
                              "D*(2010)+ : BooleanFilterCriterion/DstarFilter",
                              "D~0 : BooleanFilterCriterion/D0Filter",
                              "D0 : BooleanFilterCriterion/D0Filter",
                              "pi- : BooleanFilterCriterion",
                              "pi+ : BooleanFilterCriterion"};
HLTDstar2D02HH.DstarFilter.AndList = {"VtxFilterCriterion"};
HLTDstar2D02HH.DstarFilter.VtxFilterCriterion.MaxChi2 = 60.; // hlt tuned: 60, hlt presel: 200, offline is 15
HLTDstar2D02HH.DstarFilter.AndList = {"MassDifferenceFilterCriterion"};
HLTDstar2D02HH.DstarFilter.MassDifferenceFilterCriterion.Window = 10.; // Thomas asked for 5
HLTDstar2D02HH.DstarFilter.MassDifferenceFilterCriterion.DaughterName =  "D~0"  ;
HLTDstar2D02HH.DstarFilter.MassDifferenceFilterCriterion.MotherName =  "D*(2010)-"  ;
HLTDstar2D02HH.D0Filter.AndList = {"KinFilterCriterion"} ;
HLTDstar2D02HH.D0Filter.KinFilterCriterion.MinPt = 2000 ; // From Thomas

//------------------------------------------------------------------------------
// HLTDstar2D02HH  !! Take care of wrong-charge too
HLTselDstar.Members += {"RefineSelection/HLTFilterDstar"};
HLTFilterDstar.PhysDesktop.InputLocations = {"Phys/HLTDstar2D02HH"};
HLTFilterDstar.ParticleNames = {"D*(2010)-", "D*(2010)+"};
HLTFilterDstar.FilterNames = {"DstarFilter", "DstarFilter"};
HLTFilterDstar.DstarFilter.CriteriaNames = {"KinFilterCriterion"} ;
HLTFilterDstar.DstarFilter.KinFilterCriterion.MinPt = 2000 ;  // From Thomas
//HLTFilterDstar.D0Filter.CriteriaNames = {"KinFilterCriterion"} ;
//HLTFilterDstar.D0Filter.KinFilterCriterion.MinPt = 2000 ; // From Thomas

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselDstar.opts

//==============================================================================
// HLT Selection of B -> H H
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselB2HH.opts


//==============================================================================
// HLT Selection of B -> H H
//==============================================================================
// Author: Luis Fernandez, 13/08/2004
// Author: P. KOppenburg,  12/10/2004

//------------------------------------------------------------------------------
// Filter All B -> HH
//------------------------------------------------------------------------------
HLTSelections.Members += {"GaudiSequencer/HLTselB2HH"};
HLTselB2HH.IgnoreFilterPassed = false ; // act as real Sequencer
HLTselB2HH.MeasureTime = true ;

//------------------------------------------------------------------------------
HLTselB2HH.Members += {"RefineSelection/HLTFilterB2HH"} ;
HLTFilterB2HH.PhysDesktop.InputLocations = { "Phys/HLTPiPiFromB0" };
HLTFilterB2HH.ParticleNames = {"B0", "B~0", "pi+", "pi-"};
HLTFilterB2HH.FilterNames = {"BFilter", "BFilter","PiFilter","PiFilter" };

HLTFilterB2HH.BFilter.CriteriaNames = {  "MassFilterCriterion",
                                         "VtxFilterCriterion",
                                         "KinFilterCriterion", 
                                         "FlightDistanceFilterCriterion",
                                         "PVIPFilterCriterion",
                                         "Momentum2FlightAngleFilterCriterion"};

HLTFilterB2HH.BFilter.MassFilterCriterion.LowerWindow = 600*MeV; // to take into account the K mass. Largely enough.
HLTFilterB2HH.BFilter.MassFilterCriterion.UpperWindow = 600*MeV; // 600 MeV upwards to take into account the 90 MeV Bs-Bd mass difference

HLTFilterB2HH.BFilter.VtxFilterCriterion.MaxChi2 = 10.; // hlt tuned: 10, hlt presel: 200, offline is 4-5

HLTFilterB2HH.BFilter.KinFilterCriterion.MinPt = 1000.; // hlt tuned: 1000 MeV, hlt presel: 0 MeV, offline is 1000-1600 MeV
HLTFilterB2HH.BFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterB2HH.BFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterB2HH.BFilter.FlightDistanceFilterCriterion.MinFSPV = 4. ; // hlt tuned: 4, hlt presel: 0, offline is 14-20
HLTFilterB2HH.BFilter.PVIPFilterCriterion.MaxIPsignif = 5.; // hlt tuned: 5, hlt presel: 200, offline is 2.25-2.75
HLTFilterB2HH.BFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterB2HH.BFilter.Momentum2FlightAngleFilterCriterion.CosAngle = 0.999; // hlt tuned: 0.999, hlt presel: -2.0, no offline cut

HLTFilterB2HH.PiFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
HLTFilterB2HH.PiFilter.KinFilterCriterion.MinPt = 800.; // hlt tuned: 800 MeV, hlt presel: 0 MeV, offline is 800-1400 MeV, depending on channel
HLTFilterB2HH.PiFilter.KinFilterCriterion.MinMomentum = 2500.; // hlt tuned: 2500 MeV, hlt presel: 0 MeV, offline is 2500-2750 MeV
HLTFilterB2HH.PiFilter.PVIPFilterCriterion.MinIPsignif = 2.; // hlt tuned: 2, hlt presel: 0, offline is 5-7

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselB2HH.opts

//==============================================================================
// HLT Selection of B_s0 -> phi(1020) phi(1020)
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBs2PhiPhi.opts


//==============================================================================
// HLT Selection of B_s0 -> phi(1020) phi(1020)
//==============================================================================
// Author: Luis Fernandez, 13/08/2004

HLTSelections.Members += {"GaudiSequencer/HLTselBs2PhiPhi"};
HLTselBs2PhiPhi.IgnoreFilterPassed = false ;   // Act as Sequencer
HLTselBs2PhiPhi.MeasureTime = true ;

//------------------------------------------------------------------------------
// Apply some pre-selection on the phi daughters (the two kaons)
HLTselBs2PhiPhi.Members += {"RefineSelection/PhiSelHLTBs2PhiPhi"} ;
PhiSelHLTBs2PhiPhi.PhysDesktop.InputLocations = {"Phys/HLTKKFromPhi"}; // filter the Phis
PhiSelHLTBs2PhiPhi.ParticleNames = {"phi(1020)", "K+", "K-"};
PhiSelHLTBs2PhiPhi.FilterNames = {"PhiFilter", "KFilter", "KFilter"};
PhiSelHLTBs2PhiPhi.KFilter.CriteriaNames = {"KinFilterCriterion", 
                                            "PVIPFilterCriterion"} ;

PhiSelHLTBs2PhiPhi.KFilter.KinFilterCriterion.MinPt = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
PhiSelHLTBs2PhiPhi.KFilter.KinFilterCriterion.MinMomentum = 1000.; // hlt tuned: 1000 MeV, hlt presel: 0 MeV, no offline cut
PhiSelHLTBs2PhiPhi.KFilter.PVIPFilterCriterion.MinIPsignif = 1.; // hlt tuned: 1, hlt presel: 0, offline is 2

PhiSelHLTBs2PhiPhi.PhiFilter.CriteriaNames = {"MassFilterCriterion", 
                                              "VtxFilterCriterion",
                                              "KinFilterCriterion", 
                                              "PVIPFilterCriterion"};

PhiSelHLTBs2PhiPhi.PhiFilter.MassFilterCriterion.Window = 24*MeV; // hlt tuned: 24 MeV, hlt presel: 50 MeV, offline is 12 MeV

PhiSelHLTBs2PhiPhi.PhiFilter.VtxFilterCriterion.MaxChi2 = 100; // hlt tuned: 100, hlt presel: 200, no offline cut

PhiSelHLTBs2PhiPhi.PhiFilter.KinFilterCriterion.MinPt = 0.; // hlt tuned: 1000 MeV, hlt presel: 0 MeV, no offline cut
PhiSelHLTBs2PhiPhi.PhiFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
PhiSelHLTBs2PhiPhi.PhiFilter.PVIPFilterCriterion.MinIPsignif = 0.0 ; // hlt tuned: 0, hlt presel: 0, no offline cut
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// HLTBs2PhiPhi
HLTselBs2PhiPhi.Members += {"CombineParticles/HLTBs2PhiPhi"};
HLTBs2PhiPhi.PhysDesktop.InputLocations = {"Phys/PhiSelHLTBs2PhiPhi"};
HLTBs2PhiPhi.DecayDescriptor = "B_s0 -> phi(1020) phi(1020)";
HLTBs2PhiPhi.Selections = {"B_s0 : BooleanFilterCriterion/Chi2Filter",
                           "phi(1020) : BooleanFilterCriterion"};

HLTBs2PhiPhi.Chi2Filter.AndList = {"VtxFilterCriterion"};
HLTBs2PhiPhi.Chi2Filter.VtxFilterCriterion.MaxChi2 = 200; // hlt tuned: 200, hlt presel: 200, offline is 100
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Filter HLTBs2PhiPhi
HLTselBs2PhiPhi.Members += {"RefineSelection/HLTFilterBs2PhiPhi"};
HLTFilterBs2PhiPhi.PhysDesktop.InputLocations = {"Phys/HLTBs2PhiPhi"};
HLTFilterBs2PhiPhi.ParticleNames = {"B_s0"};
HLTFilterBs2PhiPhi.FilterNames = {"HLTBs2PhiPhiFilter"};

HLTFilterBs2PhiPhi.HLTBs2PhiPhiFilter.CriteriaNames = {"MassFilterCriterion", 
                                                       "VtxFilterCriterion",
                                                       "KinFilterCriterion", 
                                                      "FlightDistanceFilterCriterion",
                                                      "PVIPFilterCriterion",
                                                      "Momentum2FlightAngleFilterCriterion"};

HLTFilterBs2PhiPhi.HLTBs2PhiPhiFilter.MassFilterCriterion.Window = 500*MeV; // hlt tuned: 500 MeV, hlt presel: 500 MeV, offline tight is 24 MeV, loose 1 GeV

HLTFilterBs2PhiPhi.HLTBs2PhiPhiFilter.VtxFilterCriterion.MaxChi2 = 200; // hlt tuned: 200, hlt presel: 200, offline is 100

HLTFilterBs2PhiPhi.HLTBs2PhiPhiFilter.KinFilterCriterion.MinPt = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBs2PhiPhi.HLTBs2PhiPhiFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBs2PhiPhi.HLTBs2PhiPhiFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterBs2PhiPhi.HLTBs2PhiPhiFilter.FlightDistanceFilterCriterion.MinFSPV = 5.; // hlt tuned: 5, hlt presel: 0, no offline cut
HLTFilterBs2PhiPhi.HLTBs2PhiPhiFilter.PVIPFilterCriterion.MaxIPsignif = 10.0 ; // hlt tuned: 10, hlt presel: 0, no offline cut
HLTFilterBs2PhiPhi.HLTBs2PhiPhiFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterBs2PhiPhi.HLTBs2PhiPhiFilter.Momentum2FlightAngleFilterCriterion.CosAngle = 0.999; // hlt tuned: 0.999, hlt presel: 0.0, offline is 10mrad (0.9999)
//------------------------------------------------------------------------------

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBs2PhiPhi.opts

//==============================================================================
// HLT Selection of B0 -> (D~0 -> K+ pi-) (K*(892)0 -> K+ pi-)
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBd2D02KPiKstar.opts


//==============================================================================
// HLT Selection of B0 -> (D~0 -> K+ pi-) (K*(892)0 -> K+ pi-) and cc
//==============================================================================
// Author: Luis Fernandez, 13/08/2004

HLTSelections.Members += {"GaudiSequencer/HLTselBd2D02KPiKstar"};
HLTselBd2D02KPiKstar.IgnoreFilterPassed = false ;  // act as real sequencer
HLTselBd2D02KPiKstar.MeasureTime = true ;

//------------------------------------------------------------------------------
// Apply some pre-selection on the D0 daughters 
// Do this first as it is less efficient
HLTselBd2D02KPiKstar.Members += {"RefineSelection/FilterD0ForHLTBd2D0Kstar"} ;
FilterD0ForHLTBd2D0Kstar.PhysDesktop.InputLocations = {"Phys/HLTSharedD02KPi"};

FilterD0ForHLTBd2D0Kstar.ParticleNames = {"D~0", "D0", "K+", "pi-", "pi+", "K-"};
FilterD0ForHLTBd2D0Kstar.FilterNames = {"D0Filter", "D0Filter", "Filter", "Filter", "Filter", "Filter"};

FilterD0ForHLTBd2D0Kstar.Filter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;

FilterD0ForHLTBd2D0Kstar.Filter.KinFilterCriterion.MinPt = 300.; // hlt tuned: 300 MeV, hlt presel: 0 MeV, no offline cut
FilterD0ForHLTBd2D0Kstar.Filter.KinFilterCriterion.MinMomentum = 1500.; // hlt tuned: 1500 MeV, hlt presel: 1 GeV, no offline cut
FilterD0ForHLTBd2D0Kstar.Filter.PVIPFilterCriterion.MinIPsignif = 1.5; // hlt tuned: 1.5, hlt presel: 1.0, offline is larger than 2.4

FilterD0ForHLTBd2D0Kstar.D0Filter.CriteriaNames = {"MassFilterCriterion" 
                                                  , "VtxFilterCriterion"
                                                  , "KinFilterCriterion"
//                                                  , "PVIPFilterCriterion"
//                                                 ,  "FlightDistanceFilterCriterion"
						   };

FilterD0ForHLTBd2D0Kstar.D0Filter.MassFilterCriterion.Window = 45*MeV; // hlt tuned: 45 MeV, hlt presel: 200 MeV, offline is 50 MeV (in note) or 30 MeV in TDRsel
FilterD0ForHLTBd2D0Kstar.D0Filter.VtxFilterCriterion.MaxChi2 = 30.; // hlt tuned: 30, hlt presel: 50, offline is 20
FilterD0ForHLTBd2D0Kstar.D0Filter.KinFilterCriterion.MinPt = 500.; // hlt tuned: 500 MeV, hlt presel: 0 MeV, offline is 900 MeV (in note) or 500 in TDRsel
FilterD0ForHLTBd2D0Kstar.D0Filter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
//FilterD0ForHLTBd2D0Kstar.D0Filter.PVIPFilterCriterion.MinIPsignif = 0.0;// hlt tuned: 0, hlt presel: 0, no offline cut  

//FilterD0ForHLTBd2D0Kstar.D0Filter.FlightDistanceFilterCriterion.CutBestPV = true;
//FilterD0ForHLTBd2D0Kstar.D0Filter.FlightDistanceFilterCriterion.MinFSPV = 0.; // hlt tuned: 0 MeV, hlt presel: 0, no offline cut
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Apply some pre-selection on the Kstar daughters 
HLTselBd2D02KPiKstar.Members += {"RefineSelection/FilterKstarForHLTBd2D0Kstar"} ;
FilterKstarForHLTBd2D0Kstar.PhysDesktop.InputLocations = {"Phys/HLTKPiFromKstar"};

FilterKstarForHLTBd2D0Kstar.ParticleNames = {"K*(892)0", "K*(892)~0", "K+", "pi-", "pi+", "K-"};
FilterKstarForHLTBd2D0Kstar.FilterNames = {"KstarFilter", "KstarFilter", "Filter", "Filter", "Filter", "Filter"};

FilterKstarForHLTBd2D0Kstar.Filter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;

FilterKstarForHLTBd2D0Kstar.Filter.KinFilterCriterion.MinPt = 300.; // hlt tuned: 300 MeV, hlt presel: 0 MeV, no offline cut
FilterKstarForHLTBd2D0Kstar.Filter.KinFilterCriterion.MinMomentum = 1500.; // hlt tuned: 1500 MeV, hlt presel: 1 GeV, no offline cut
FilterKstarForHLTBd2D0Kstar.Filter.PVIPFilterCriterion.MinIPsignif = 1.5; // hlt tuned: 1.5, hlt presel: 1.0, offline is 2.4
FilterKstarForHLTBd2D0Kstar.KstarFilter.CriteriaNames = {"MassFilterCriterion", 
                                                         "VtxFilterCriterion",
                                                         "KinFilterCriterion", 
                                                         "PVIPFilterCriterion",
                                                         "FlightDistanceFilterCriterion"};

FilterKstarForHLTBd2D0Kstar.KstarFilter.MassFilterCriterion.Window = 120*MeV; // hlt tuned: 120 MeV, hlt presel: 200 MeV, offline is 80 MeV (in note) 

FilterKstarForHLTBd2D0Kstar.KstarFilter.VtxFilterCriterion.MaxChi2 = 30.; // hlt tuned: 30, hlt presel: 50, offline is 20

FilterKstarForHLTBd2D0Kstar.KstarFilter.KinFilterCriterion.MinPt = 500.0; // hlt tuned: 500 MeV, hlt presel: 0 MeV, offline is 900 MeV (in note) or 500 in TDRsel
FilterKstarForHLTBd2D0Kstar.KstarFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
FilterKstarForHLTBd2D0Kstar.KstarFilter.PVIPFilterCriterion.MinIPsignif = 0.0 ; // hlt tuned: 0 MeV, hlt presel: 0, no offline cut

FilterKstarForHLTBd2D0Kstar.KstarFilter.FlightDistanceFilterCriterion.CutBestPV = true;
FilterKstarForHLTBd2D0Kstar.KstarFilter.FlightDistanceFilterCriterion.MinFSPV = 0.0; // hlt tuned: 0, hlt presel: 0, no offline cut
//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
// Bd2D0Kstar 
HLTselBd2D02KPiKstar.Members += {"CombineParticles/HLTBd2D02KPiKstar"};
HLTBd2D02KPiKstar.PhysDesktop.InputLocations = {"Phys/FilterKstarForHLTBd2D0Kstar", "Phys/FilterD0ForHLTBd2D0Kstar"};

HLTBd2D02KPiKstar.DecayDescriptor = "[B0 -> D~0 K*(892)0]cc";
HLTBd2D02KPiKstar.Selections = {"B0 : BooleanFilterCriterion/Chi2Filter",
                                "B~0 : BooleanFilterCriterion/Chi2Filter",
                                "D~0 : BooleanFilterCriterion",
                                "D0 : BooleanFilterCriterion",
                                "K*(892)0 : BooleanFilterCriterion",
                                "K*(892)~0 : BooleanFilterCriterion"};

HLTBd2D02KPiKstar.Chi2Filter.AndList = {"VtxFilterCriterion"};
HLTBd2D02KPiKstar.Chi2Filter.VtxFilterCriterion.MaxChi2 = 200.; // hlt tuned: 200, hlt presel: 200, offline is 10 (in note) or 15 in TDRsel
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Filter Bd2D0Kstar
HLTselBd2D02KPiKstar.Members += {"RefineSelection/HLTFilterBd2D02KPiKstar"};
HLTFilterBd2D02KPiKstar.PhysDesktop.InputLocations = {"Phys/HLTBd2D02KPiKstar"};

HLTFilterBd2D02KPiKstar.ParticleNames = {"B0", "B~0"};
HLTFilterBd2D02KPiKstar.FilterNames = {"BFilter", "BFilter"};


HLTFilterBd2D02KPiKstar.BFilter.CriteriaNames = {"MassFilterCriterion", 
                                                                 "VtxFilterCriterion",
                                                                 "KinFilterCriterion", 
                                                                 "FlightDistanceFilterCriterion",
                                                                 "PVIPFilterCriterion",
                                                                 "Momentum2FlightAngleFilterCriterion"};

HLTFilterBd2D02KPiKstar.BFilter.MassFilterCriterion.Window = 500*MeV; // hlt tuned: 500 MeV, hlt presel: 500 MeV, offline tight is 50 MeV, loose is 500 MeV

HLTFilterBd2D02KPiKstar.BFilter.VtxFilterCriterion.MaxChi2 = 200.; // hlt tuned: 200, hlt presel: 200, offline is 10 (in note) or 15 in TDRsel

HLTFilterBd2D02KPiKstar.BFilter.KinFilterCriterion.MinPt = 5000.; // hlt tuned: 5 GeV, hlt presel: 0 MeV, no offline cut
HLTFilterBd2D02KPiKstar.BFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBd2D02KPiKstar.BFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterBd2D02KPiKstar.BFilter.FlightDistanceFilterCriterion.MinFSPV = 6.; // hlt tuned: 6, hlt presel: 0, no offline cut
HLTFilterBd2D02KPiKstar.BFilter.PVIPFilterCriterion.MaxIPsignif = 10. ; // hlt tuned: 10, hlt presel: 20, offline is 3.8 (in note) or 4.0 in TDRsel
HLTFilterBd2D02KPiKstar.BFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterBd2D02KPiKstar.BFilter.Momentum2FlightAngleFilterCriterion.CosAngle = 0.999; // hlt tuned: 0.999, hlt presel: 0.0, offline is 45mrad or 0.9999
//------------------------------------------------------------------------------

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBd2D02KPiKstar.opts

//==============================================================================
// HLT Selection of B0 -> (D*(2010)- -> (D~0 -> K+ pi-)  pi-) pi+ 
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBd2DstarPi.opts


//==============================================================================
// HLT Selection of B0 -> (D*(2010)- -> (D~0 -> K+ pi-)  pi-) pi+ 
//==============================================================================
// Author: Luis Fernandez, 13/08/2004

HLTSelections.Members += {"GaudiSequencer/HLTselBd2DstarPi"};
HLTselBd2DstarPi.IgnoreFilterPassed = false ;  // act as real sequencer
HLTselBd2DstarPi.MeasureTime = true ;

//------------------------------------------------------------------------------
// Apply some pre-selection on the D0 daughters 
HLTselBd2DstarPi.Members += { "RefineSelection/FilterD0ForHLTBd2DstarPi" } ;
FilterD0ForHLTBd2DstarPi.PhysDesktop.InputLocations = { "Phys/HLTSharedD02KPi"};
FilterD0ForHLTBd2DstarPi.ParticleNames = { "D0", "D~0", "K+", "K-", "pi-", "pi+" };
FilterD0ForHLTBd2DstarPi.FilterNames = {"D0Filter", "D0Filter", "Filter", "Filter", "Filter", "Filter"};

FilterD0ForHLTBd2DstarPi.Filter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
FilterD0ForHLTBd2DstarPi.Filter.KinFilterCriterion.MinPt = 300. ; // hlt tuned: 300 MeV, hlt presel: 0 MeV, offline cuts are 150 MeV for pi and 200 MeV for K
FilterD0ForHLTBd2DstarPi.Filter.KinFilterCriterion.MinMomentum = 1000. ; // hlt tuned: 1 GeV, hlt presel: 1 GeV, no offline cut
FilterD0ForHLTBd2DstarPi.Filter.PVIPFilterCriterion.MinIPsignif = 1.; // hlt tuned: 1, hlt presel: 0, no offline cut

FilterD0ForHLTBd2DstarPi.D0Filter.CriteriaNames = {"MassFilterCriterion"
                                                  ,  "VtxFilterCriterion"
                                                  ,  "KinFilterCriterion"
//                                                   ,"PVIPFilterCriterion"
//                                                   ,"FlightDistanceFilterCriterion"
                                                   };
FilterD0ForHLTBd2DstarPi.D0Filter.MassFilterCriterion.Window = 30*MeV; // hlt tuned: 30 MeV, hlt presel: 200 MeV, offline is 11 MeV
FilterD0ForHLTBd2DstarPi.D0Filter.VtxFilterCriterion.MaxChi2 = 80.; // hlt tuned: 80, hlt presel: 200, offline is 40
FilterD0ForHLTBd2DstarPi.D0Filter.KinFilterCriterion.MinPt = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
FilterD0ForHLTBd2DstarPi.D0Filter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
//FilterD0ForHLTBd2DstarPi.D0Filter.PVIPFilterCriterion.MinIPsignif = 0.0 ; // hlt tuned: 0, hlt presel: 0,no offline cut
//FilterD0ForHLTBd2DstarPi.D0Filter.FlightDistanceFilterCriterion.CutBestPV = true;
//FilterD0ForHLTBd2DstarPi.D0Filter.FlightDistanceFilterCriterion.MinFSPV = 0.; // hlt tuned: 0, hlt presel: 0, no offline cut
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Apply some pre-selection cuts to the soft pion
// Maybe use upstream tracks?
HLTselBd2DstarPi.Members += {"RefineSelection/SoftPionForHLTDstar2D02KPi"};
SoftPionForHLTDstar2D02KPi.PhysDesktop.InputLocations = {"Phys/HLTPions"};
SoftPionForHLTDstar2D02KPi.ParticleNames = {"pi+", "pi-"};
SoftPionForHLTDstar2D02KPi.FilterNames = {"HLTPiFilter", "HLTPiFilter"};
SoftPionForHLTDstar2D02KPi.HLTPiFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
SoftPionForHLTDstar2D02KPi.HLTPiFilter.KinFilterCriterion.MinPt = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
SoftPionForHLTDstar2D02KPi.HLTPiFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
SoftPionForHLTDstar2D02KPi.HLTPiFilter.PVIPFilterCriterion.MaxIP = 60.; // hlt tuned: 60mm, hlt presel: 60mm, offline cut is 9mm
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// HLTDstar2D02KPi
HLTselBd2DstarPi.Members += {"CombineParticles/HLTDstar2D02KPi"};
HLTDstar2D02KPi.PhysDesktop.InputLocations = {"Phys/FilterD0ForHLTBd2DstarPi", "Phys/SoftPionForHLTDstar2D02KPi"};
HLTDstar2D02KPi.DecayDescriptor = " [ D*(2010)- -> D~0 pi- ]cc ";
HLTDstar2D02KPi.Selections = {"D*(2010)- : BooleanFilterCriterion/Chi2Filter",
                              "D*(2010)+ : BooleanFilterCriterion/Chi2Filter",
                              "D~0 : BooleanFilterCriterion",
                              "D0 : BooleanFilterCriterion",
                              "pi- : BooleanFilterCriterion",
                              "pi+ : BooleanFilterCriterion"};
HLTDstar2D02KPi.Chi2Filter.AndList = {"VtxFilterCriterion"};
HLTDstar2D02KPi.Chi2Filter.VtxFilterCriterion.MaxChi2 = 60.; // hlt tuned: 60, hlt presel: 200, offline is 15
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Filter HLTDstar2D02KPi
HLTselBd2DstarPi.Members += {"RefineSelection/FilterDstarForHLTBd2DstarPi"} ;
FilterDstarForHLTBd2DstarPi.PhysDesktop.InputLocations = {"Phys/HLTDstar2D02KPi"};
FilterDstarForHLTBd2DstarPi.ParticleNames = { "D*(2010)-", "D*(2010)+" };
FilterDstarForHLTBd2DstarPi.FilterNames = {"HLTDstar2D02KPiFilter", "HLTDstar2D02KPiFilter"};


// For D*(2010)-
FilterDstarForHLTBd2DstarPi.HLTDstar2D02KPiFilter.CriteriaNames = {//"MassDifferenceFilterCriterion",
                                                                             "MassFilterCriterion",
                                                                             "VtxFilterCriterion",
                                                                             "KinFilterCriterion",
                                                                             "PVIPFilterCriterion",
                                                                             "FlightDistanceFilterCriterion"};

// Do we need two different filters because of MotherName and DaughterName???

FilterDstarForHLTBd2DstarPi.HLTDstar2D02KPiFilter.MassFilterCriterion.Window = 40*MeV; // hlt tuned: 40 MeV, hlt presel: 200 MeV, offline is 150 MeV for D*
// FilterDstarForHLTBd2DstarPi.HLTDstar2D02KPiFilter.MassDifferenceFilterCriterion.Window = 4*MeV; // offline is 3.5
// FilterDstarForHLTBd2DstarPi.HLTDstar2D02KPiFilter.MassDifferenceFilterCriterion.MotherName = "D*(2010)-"; 
// FilterDstarForHLTBd2DstarPi.HLTDstar2D02KPiFilter.MassDifferenceFilterCriterion.DaughterName = "D~0"; 

FilterDstarForHLTBd2DstarPi.HLTDstar2D02KPiFilter.VtxFilterCriterion.MaxChi2 = 60; // hlt tuned: 60, hlt presel: 200, offline is 15
FilterDstarForHLTBd2DstarPi.HLTDstar2D02KPiFilter.KinFilterCriterion.MinPt = 500.; // hlt tuned: 500 MeV, hlt presel: 0 MeV, offline is 1400 MeV
FilterDstarForHLTBd2DstarPi.HLTDstar2D02KPiFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
FilterDstarForHLTBd2DstarPi.HLTDstar2D02KPiFilter.PVIPFilterCriterion.MinIPsignif = 0.0 ; // hlt tuned: 0, hlt presel: 0, no offline cut
FilterDstarForHLTBd2DstarPi.HLTDstar2D02KPiFilter.FlightDistanceFilterCriterion.CutBestPV = true;
FilterDstarForHLTBd2DstarPi.HLTDstar2D02KPiFilter.FlightDistanceFilterCriterion.MinFSPV = 0.; // hlt tuned: 0, hlt presel: 0, no offline cut
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Apply some pre-selection cuts to the hard pion
HLTselBd2DstarPi.Members += {"RefineSelection/HardPionForHLTDstar2D02KPi"};
HardPionForHLTDstar2D02KPi.PhysDesktop.InputLocations = {"Phys/HLTPreselPions"};
HardPionForHLTDstar2D02KPi.ParticleNames = {"pi+", "pi-"};
HardPionForHLTDstar2D02KPi.FilterNames = {"HLTPiFilter", "HLTPiFilter"};
HardPionForHLTDstar2D02KPi.HLTPiFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
HardPionForHLTDstar2D02KPi.HLTPiFilter.KinFilterCriterion.MinPt = 1300.; // hlt tuned: 1300 MeV, hlt presel: 300 MeV, offline is 1300 MeV
HardPionForHLTDstar2D02KPi.HLTPiFilter.KinFilterCriterion.MinMomentum = 1000.; // hlt tuned: 1000 MeV, hlt presel: 1000 MeV, no offline cut
HardPionForHLTDstar2D02KPi.HLTPiFilter.PVIPFilterCriterion.MinIP = -1000.; // hlt tuned: -1000, hlt presel: -1000, offline is 0.05 mm
HardPionForHLTDstar2D02KPi.HLTPiFilter.PVIPFilterCriterion.MinIPsignif = 2.5; // hlt tuned: 2.5, hlt presel: 0.5, offline is 0.05 mm
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// HLTBd2DstarPi
HLTselBd2DstarPi.Members += {"CombineParticles/HLTBd2DstarPi"};
HLTBd2DstarPi.PhysDesktop.InputLocations = {"Phys/FilterDstarForHLTBd2DstarPi", "Phys/HardPionForHLTDstar2D02KPi"};
HLTBd2DstarPi.DecayDescriptor = "[B0 -> D*(2010)- pi+]cc";
HLTBd2DstarPi.Selections = {"B0 : BooleanFilterCriterion/Chi2Filter",
                            "B~0 : BooleanFilterCriterion/Chi2Filter",
                            "D*(2010)- : BooleanFilterCriterion",
                            "D*(2010)+ : BooleanFilterCriterion",
                            "pi+ : BooleanFilterCriterion",
                            "pi-: BooleanFilterCriterion"};

HLTBd2DstarPi.Chi2Filter.AndList = {"VtxFilterCriterion"};
HLTBd2DstarPi.Chi2Filter.VtxFilterCriterion.MaxChi2 =50.; // hlt tuned: 50, hlt presel: 200, offline is 12
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Filter HLTBd2DstarPi
HLTselBd2DstarPi.Members += {"RefineSelection/HLTFilterBd2DstarPi"} ;
HLTFilterBd2DstarPi.PhysDesktop.InputLocations = {"Phys/HLTBd2DstarPi"};
HLTFilterBd2DstarPi.ParticleNames = {"B0", "B~0"};
HLTFilterBd2DstarPi.FilterNames = {"HLTBd2DstarPiFilter", "HLTBd2DstarPiFilter"};


HLTFilterBd2DstarPi.HLTBd2DstarPiFilter.CriteriaNames = {"MassFilterCriterion",
                                                         "VtxFilterCriterion",
                                                         "KinFilterCriterion",
                                                         "PVIPFilterCriterion",
                                                         "FlightDistanceFilterCriterion",
                                                         "Momentum2FlightAngleFilterCriterion"};

HLTFilterBd2DstarPi.HLTBd2DstarPiFilter.MassFilterCriterion.Window = 500*MeV; // hlt tuned: 500 MeV, hlt presel: 500 MeV, offline tight is 50 MeV, loose is 200 MeV

HLTFilterBd2DstarPi.HLTBd2DstarPiFilter.VtxFilterCriterion.MaxChi2 = 50.; // hlt tuned: 50, hlt presel: 200, offline is 12

HLTFilterBd2DstarPi.HLTBd2DstarPiFilter.KinFilterCriterion.MinPt = 1000.; // hlt tuned: 1000 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBd2DstarPi.HLTBd2DstarPiFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut

HLTFilterBd2DstarPi.HLTBd2DstarPiFilter.PVIPFilterCriterion.MaxIPsignif = 6.0; // hlt tuned: 6, hlt presel: 1000, offline is 0.05 mm
HLTFilterBd2DstarPi.HLTBd2DstarPiFilter.PVIPFilterCriterion.MaxIP = 30.; // hlt presel: 30, offline is 0.05 mm

HLTFilterBd2DstarPi.HLTBd2DstarPiFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterBd2DstarPi.HLTBd2DstarPiFilter.FlightDistanceFilterCriterion.MinFSPV = 5.; // hlt tuned: 5, hlt presel: 0 , no offline cut

HLTFilterBd2DstarPi.HLTBd2DstarPiFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterBd2DstarPi.HLTBd2DstarPiFilter.Momentum2FlightAngleFilterCriterion.CosAngle = 0.999; // hlt tuned: 0.999, hlt presel: 0.0, no offline cut
//------------------------------------------------------------------------------

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBd2DstarPi.opts

// For MC truth
// HLTSoftPionForHLTDstar.PhysDesktop.InputLocations = {"Phys/SelectBd2DstarPi"};
//==============================================================================
// HLT Selection of B_s0 -> (D_s- -> K+ K- pi-) h+
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBs2DsH.opts


//==============================================================================
// HLT Selection of B_s0 -> (D_s- -> K+ K- pi-) h+
//==============================================================================
// Author: Luis Fernandez, 13/08/2004
//==============================================================================

// --> Including file $HLTSELECTIONSROOT/options/HLTMakeDs.opts


//==============================================================================
// Form the D_s- -> K+ K- pi- and cc for Bs2DsH
//==============================================================================

HLTSelections.Members += {"GaudiSequencer/HLTMakeDs"};
HLTMakeDs.IgnoreFilterPassed = true ;
HLTMakeDs.MeasureTime = true ;

//------------------------------------------------------------------------------
// Duplicate pions as kaons (for the Ds!)
HLTMakeDs.Members += {"ChangePID/HLTKaons"};
HLTKaons.PhysDesktop.InputLocations = {"Phys/HLTPions"};
HLTKaons.Particles = {"pi+", "pi-"};
HLTKaons.Replacements = {"K+", "K-"};
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Apply some pre-selection cuts on kaons (for symmetry with pions, but not used so far).
HLTMakeDs.Members += {"RefineSelection/HLTPreselKaons"};
HLTPreselKaons.PhysDesktop.InputLocations = {"Phys/HLTKaons"};
HLTPreselKaons.ParticleNames = {"K+", "K-"};
HLTPreselKaons.FilterNames = {"KFilter", "KFilter"};
HLTPreselKaons.KFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
HLTPreselKaons.KFilter.KinFilterCriterion.MinPt = 300.; // MeV
HLTPreselKaons.KFilter.KinFilterCriterion.MinMomentum = 0.; // MeV
HLTPreselKaons.KFilter.PVIPFilterCriterion.MinIPsignif = 0.5; // sigma
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Apply some pre-selection on the daughters of the Ds
HLTMakeDs.Members += {"RefineSelection/HLTDaughtersFromDs"} ;
HLTDaughtersFromDs.PhysDesktop.InputLocations = {"Phys/HLTPreselKaons", "Phys/HLTPreselPions"};
HLTDaughtersFromDs.ParticleNames = {"K+", "K-", "pi+", "pi-"};
HLTDaughtersFromDs.FilterNames = {"KFilter", "KFilter", "PiFilter", "PiFilter"};

HLTDaughtersFromDs.KFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;

HLTDaughtersFromDs.KFilter.KinFilterCriterion.MinPt = 300.0; // hlt tuned: 300 MeV, hlt presel: 300 MeV , offline is 300 MeV
HLTDaughtersFromDs.KFilter.KinFilterCriterion.MinMomentum = 1500.0; // hlt tuned: 1.5 GeV, hlt presel:1 GeV, offline is 2 GeV
HLTDaughtersFromDs.KFilter.PVIPFilterCriterion.MinIPsignif = 1.; // hlt tuned: 1, hlt presel: 0, no offline cut

HLTDaughtersFromDs.PiFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
HLTDaughtersFromDs.PiFilter.KinFilterCriterion.MinPt = 300.0; // hlt tuned: 300 MeV, hlt presel: 300 MeV, offline is 300 MeV
HLTDaughtersFromDs.PiFilter.KinFilterCriterion.MinMomentum = 1500.0; // hlt tuned: 1.5 MeV, hlt presel: 1 GeV, offline is 2 GeV
HLTDaughtersFromDs.PiFilter.PVIPFilterCriterion.MinIPsignif = 1.; // hlt tuned: 1, hlt presel: 0, no offline cut
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// THREE-BODY DECAY Ds, simple but CPU-consuming
//------------------------------------------------------------------------------
// HLTDs2KKPi
//HLTMakeDs.Members += {"CombineParticles/HLTDs2KKPi"};
HLTDs2KKPi.PhysDesktop.InputLocations = {"Phys/HLTDaughtersFromDs"};
HLTDs2KKPi.DecayDescriptor = "[D_s- -> K+ K- pi-]cc";
HLTDs2KKPi.Selections = {"D_s- : BooleanFilterCriterion/DsFilter",
                         "D_s+ : BooleanFilterCriterion/DsFilter",
                         "K+ : BooleanFilterCriterion",
                         "K- : BooleanFilterCriterion",
                         "pi+ : BooleanFilterCriterion",
                         "pi- : BooleanFilterCriterion"};

HLTDs2KKPi.DsFilter.AndList = {"VtxFilterCriterion" };
HLTDs2KKPi.DsFilter.VtxFilterCriterion.MaxChi2 = 40; // hlt tuned: 40, hlt presel: 100, offline is 20
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// 2-BODY DECAY Ds, more complicated
//------------------------------------------------------------------------------
// filter K* and Phi's as in HLTDaughtersFromDs -> CAN BE MERGED WITH IT LATER
//------------------------------------------------------------------------------
// Apply some pre-selection on the daughters of the Ds
HLTMakeDs.Members += {"RefineSelection/HLTResonancesFromDs"} ;
HLTResonancesFromDs.PhysDesktop.InputLocations = {"Phys/HLTKPiFromKstar", "Phys/HLTKKFromPhi"}; 
HLTResonancesFromDs.ParticleNames = {"K*(892)0", "K*(892)~0", "phi(1020)", "K+", "K-", "pi+", "pi-"};  // filter on daughters and on mothers
HLTResonancesFromDs.FilterNames = {"KstarFilter", "KstarFilter", "PhiFilter", "KFilter", "KFilter", "PiFilter", "PiFilter"};

HLTResonancesFromDs.KstarFilter.CriteriaNames = {"MassFilterCriterion"} ;
HLTResonancesFromDs.KstarFilter.MassFilterCriterion.Window = 120*MeV ; // hlt tuned: 120 MeV, hlt presel: 200 MeV, offline is 80 MeV (in note) 

HLTResonancesFromDs.PhiFilter.CriteriaNames = {"MassFilterCriterion"} ;
HLTResonancesFromDs.PhiFilter.MassFilterCriterion.Window = 24*MeV ; // hlt tuned: 24 MeV, hlt presel: 50 MeV, offline is 12 MeV

HLTResonancesFromDs.KFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
HLTResonancesFromDs.KFilter.KinFilterCriterion.MinPt = 300.0; // hlt tuned: 300 MeV, hlt presel: 300 MeV , offline is 300 MeV
HLTResonancesFromDs.KFilter.KinFilterCriterion.MinMomentum = 1500.0; // hlt tuned: 1.5 GeV, hlt presel:1 GeV, offline is 2 GeV
HLTResonancesFromDs.KFilter.PVIPFilterCriterion.MinIPsignif = 1.; // hlt tuned: 1, hlt presel: 0, no offline cut

HLTResonancesFromDs.PiFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
HLTResonancesFromDs.PiFilter.KinFilterCriterion.MinPt = 300.0; // hlt tuned: 300 MeV, hlt presel: 300 MeV, offline is 300 MeV
HLTResonancesFromDs.PiFilter.KinFilterCriterion.MinMomentum = 1500.0; // hlt tuned: 1.5 MeV, hlt presel: 1 GeV, offline is 2 GeV
HLTResonancesFromDs.PiFilter.PVIPFilterCriterion.MinIPsignif = 1.; // hlt tuned: 1, hlt presel: 0, no offline cut
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Make Ds -> K*K
HLTMakeDs.Members += {"CombineParticles/HLTDs2KstarK"};
HLTDs2KstarK.PhysDesktop.InputLocations = {"Phys/HLTDaughtersFromDs", "Phys/HLTResonancesFromDs" };
HLTDs2KstarK.DecayDescriptor = "[D_s- -> K*(892)0 K- ]cc";
HLTDs2KstarK.Selections = {"D_s- : BooleanFilterCriterion/DsFilter",
                         "D_s+ : BooleanFilterCriterion/DsFilter",
                         "K*(892)0 : BooleanFilterCriterion",
                         "K*(892)~0 : BooleanFilterCriterion",
                         "K+ : BooleanFilterCriterion",
                         "K- : BooleanFilterCriterion"};

HLTDs2KstarK.DsFilter.AndList = {"VtxFilterCriterion" };
HLTDs2KstarK.DsFilter.VtxFilterCriterion.MaxChi2 = 40; // hlt tuned: 40, hlt presel: 100, offline is 20
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Make Ds -> phi pi
HLTMakeDs.Members += {"CombineParticles/HLTDs2PhiPi"};
HLTDs2PhiPi.PhysDesktop.InputLocations = {"Phys/HLTDaughtersFromDs", "Phys/HLTResonancesFromDs" };
HLTDs2PhiPi.DecayDescriptor = "[D_s- -> phi(1020) pi- ]cc";
HLTDs2PhiPi.Selections = {"D_s- : BooleanFilterCriterion/DsFilter",
                         "D_s+ : BooleanFilterCriterion/DsFilter",
                         "phi(1020) : BooleanFilterCriterion",
                         "pi+ : BooleanFilterCriterion",
                         "pi- : BooleanFilterCriterion"};

HLTDs2PhiPi.DsFilter.AndList = {"VtxFilterCriterion" };
HLTDs2PhiPi.DsFilter.VtxFilterCriterion.MaxChi2 = 40; // hlt tuned: 40, hlt presel: 100, offline is 20
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Filter HLTDs2KKPi
HLTMakeDs.Members += {"RefineSelection/HLTFilterDs2KKPi"};
//HLTFilterDs2KKPi.PhysDesktop.InputLocations = {"Phys/HLTDs2KKPi"}; // 3-body case
HLTFilterDs2KKPi.PhysDesktop.InputLocations = {"Phys/HLTDs2PhiPi", "Phys/HLTDs2KstarK"}; // 2*2-body case
HLTFilterDs2KKPi.ParticleNames = {"D_s-", "D_s+"};
HLTFilterDs2KKPi.FilterNames = {"HLTDs2KKPiFilter", "HLTDs2KKPiFilter"};

HLTFilterDs2KKPi.HLTDs2KKPiFilter.CriteriaNames = {"MassFilterCriterion", 
                                                   "VtxFilterCriterion",
                                                   "KinFilterCriterion", 
                                                  "FlightDistanceFilterCriterion",
                                                  "PVIPFilterCriterion",
                                                  "Momentum2FlightAngleFilterCriterion"};

HLTFilterDs2KKPi.HLTDs2KKPiFilter.MassFilterCriterion.Window = 45*MeV; // hlt tuned: 45 MeV, hlt presel: 100 MeV, offline is 15 MeV

HLTFilterDs2KKPi.HLTDs2KKPiFilter.VtxFilterCriterion.MaxChi2 = 40; // hlt tuned: 40, hlt presel: 100, offline is 20

HLTFilterDs2KKPi.HLTDs2KKPiFilter.KinFilterCriterion.MinPt = 2500.; // hlt tuned: 2500 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterDs2KKPi.HLTDs2KKPiFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterDs2KKPi.HLTDs2KKPiFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterDs2KKPi.HLTDs2KKPiFilter.FlightDistanceFilterCriterion.MinFSPV = 3.0; // hlt tuned: 3, hlt presel: 0, offline cut is 4.5
HLTFilterDs2KKPi.HLTDs2KKPiFilter.PVIPFilterCriterion.MinIPsignif = 0.0 ; // hlt tuned: 0, hlt presel: 0, offline cut is 2.0 but w.r.t best PV ...
HLTFilterDs2KKPi.HLTDs2KKPiFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterDs2KKPi.HLTDs2KKPiFilter.Momentum2FlightAngleFilterCriterion.CosAngle = -2.0; // hlt tuned: -2.0, hlt presel: -2.0, no offline cut
//------------------------------------------------------------------------------


// <-- End of including file $HLTSELECTIONSROOT/options/HLTMakeDs.opts


HLTSelections.Members += {"GaudiSequencer/HLTselBs2DsH"};
HLTselBs2DsH.IgnoreFilterPassed = false ;
HLTselBs2DsH.MeasureTime = true ;

//==============================================================================
// B_s0 -> (D_s- -> K+ K- pi-) [pi+,K+] 
//==============================================================================

//------------------------------------------------------------------------------
// Look for the bachelor pion for HLTBs2DsPi
HLTselBs2DsH.Members += {"RefineSelection/HLTBachelorForHLTBs2DsH"};
HLTBachelorForHLTBs2DsH.PhysDesktop.InputLocations = {"Phys/HLTPreselPions"};
HLTBachelorForHLTBs2DsH.ParticleNames = {"pi+", "pi-"};
HLTBachelorForHLTBs2DsH.FilterNames = {"HFilter", "HFilter"};
HLTBachelorForHLTBs2DsH.HFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
HLTBachelorForHLTBs2DsH.HFilter.KinFilterCriterion.MinPt = 1300.; // hlt tuned: 1300 MeV, hlt presel: 300 MeV, offline is 700 MeV
HLTBachelorForHLTBs2DsH.HFilter.KinFilterCriterion.MinMomentum = 2500.; // hlt tuned: 2.5 GeV, hlt presel: 1 GeV, offline is 2 GeV
HLTBachelorForHLTBs2DsH.HFilter.PVIPFilterCriterion.MinIPsignif = 3.; // hlt tuned: 3, hlt presel: 1, offline is 4
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// HLTBs2DsH
HLTselBs2DsH.Members += {"CombineParticles/HLTBs2DsH"};
HLTBs2DsH.PhysDesktop.InputLocations = {"Phys/HLTFilterDs2KKPi", "Phys/HLTBachelorForHLTBs2DsH"};
HLTBs2DsH.DecayDescriptor = "[B_s0 -> D_s- pi+]cc";
HLTBs2DsH.Selections = {"B_s0 : BooleanFilterCriterion/Chi2Filter",
                        "B_s~0 : BooleanFilterCriterion/Chi2Filter",
                        "D_s- : BooleanFilterCriterion",
                        "D_s+ : BooleanFilterCriterion",
                        "pi+ : BooleanFilterCriterion",
                        "pi- : BooleanFilterCriterion"};

HLTBs2DsH.Chi2Filter.AndList = {"VtxFilterCriterion"};
HLTBs2DsH.Chi2Filter.VtxFilterCriterion.MaxChi2 = 12; // hlt tuned:12, hlt presel: 100, offline is 3
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Filter HLTBs2DsH
HLTselBs2DsH.Members += {"RefineSelection/HLTFilterBs2DsH"} ;
HLTFilterBs2DsH.PhysDesktop.InputLocations = {"Phys/HLTBs2DsH"};
HLTFilterBs2DsH.ParticleNames = {"B_s0", "B_s~0"};
HLTFilterBs2DsH.FilterNames = {"BsFilter", "BsFilter"};

HLTFilterBs2DsH.BsFilter.CriteriaNames = {"MassFilterCriterion", 
                                          "VtxFilterCriterion",
                                          "KinFilterCriterion", 
                                          "PVIPFilterCriterion",
                                          "FlightDistanceFilterCriterion",
                                          "Momentum2FlightAngleFilterCriterion"};

HLTFilterBs2DsH.BsFilter.MassFilterCriterion.UpperWindow = 500*MeV; // hlt tuned: 500 MeV, hlt presel: 500 MeV, offline tight is 50 MeV, loose is 500 MeV
HLTFilterBs2DsH.BsFilter.MassFilterCriterion.LowerWindow = 600*MeV; // To take into account Bs2DsK

HLTFilterBs2DsH.BsFilter.VtxFilterCriterion.MaxChi2 = 12; // hlt tuned: 12, hlt presel: 100, offline is 3

HLTFilterBs2DsH.BsFilter.KinFilterCriterion.MinPt = 3000.; // hlt tuned: 3000 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBs2DsH.BsFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBs2DsH.BsFilter.PVIPFilterCriterion.MaxIPsignif = 8.0 ; // hlt tuned: 8, hlt presel: 1000, offline is 3

HLTFilterBs2DsH.BsFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterBs2DsH.BsFilter.FlightDistanceFilterCriterion.MinFSPV = 6.; // hlt tuned: 6, hlt presel: 0, no offline cut

HLTFilterBs2DsH.BsFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterBs2DsH.BsFilter.Momentum2FlightAngleFilterCriterion.CosAngle = 0.9995; // hlt tuned: 0.9995, hlt presel: 0.0, offline is 8 mrad
//------------------------------------------------------------------------------



// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBs2DsH.opts

//==============================================================================
// HLT Selection of B_s0 -> Mu+ Mu-
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselB2MuMu.opts


//==============================================================================
// HLT Selection of B -> Mu Mu
//==============================================================================
// Author: P. Koppenburg,  28/10/2004

//------------------------------------------------------------------------------
// Filter All B -> HH
//------------------------------------------------------------------------------
HLTSelections.Members += {"GaudiSequencer/HLTselB2MuMu"};
HLTselB2MuMu.IgnoreFilterPassed = false ; // act as real Sequencer
HLTselB2MuMu.MeasureTime = true ;

//------------------------------------------------------------------------------
// Transform Psi vertex into a Bs0 
HLTselB2MuMu.Members += {"ChangePID/HLTB2MuMu"};   
HLTB2MuMu.PhysDesktop.InputLocations = {"Phys/HLTDimuons"};
HLTB2MuMu.Particles = { "J/psi(1S)" } ;
HLTB2MuMu.Replacements = { "B_s0"} ;
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
HLTselB2MuMu.Members += {"RefineSelection/HLTFilterB2MuMu"} ;
HLTFilterB2MuMu.PhysDesktop.InputLocations = { "Phys/HLTB2MuMu" };
HLTFilterB2MuMu.ParticleNames = {"B_s0", "mu+", "mu-"};
HLTFilterB2MuMu.FilterNames = {"BFilter","MuFilter","MuFilter" };

HLTFilterB2MuMu.BFilter.CriteriaNames = {  "MassFilterCriterion"
                                         ,"VtxFilterCriterion"
                                         ,"KinFilterCriterion"
                                        , "FlightDistanceFilterCriterion"
//                                         ,"PVIPFilterCriterion"
					 };

HLTFilterB2MuMu.BFilter.MassFilterCriterion.Window = 500*MeV; // offline: 60 MeV

HLTFilterB2MuMu.BFilter.VtxFilterCriterion.MaxChi2 = 50.; // offline: 4

HLTFilterB2MuMu.BFilter.KinFilterCriterion.MinPt = 2000.; // offline: 3 GeV
HLTFilterB2MuMu.BFilter.KinFilterCriterion.MinMomentum = 0.; // no offline cut
HLTFilterB2MuMu.BFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterB2MuMu.BFilter.FlightDistanceFilterCriterion.MinFSPV = 3. ; // offline: 29
//HLTFilterB2MuMu.BFilter.PVIPFilterCriterion.MaxIPsignif = 10.0 ; //offline: 3.5

HLTFilterB2MuMu.MuFilter.CriteriaNames = {"KinFilterCriterion", "PVIPFilterCriterion"} ;
HLTFilterB2MuMu.MuFilter.KinFilterCriterion.MinPt = 1000.; // offline: 1.3 GeV
HLTFilterB2MuMu.MuFilter.PVIPFilterCriterion.MinIPsignif = 0.; // no offline cut

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselB2MuMu.opts

//==============================================================================
// HLT Selection of B0 -> Mu+ Mu- K*
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBd2MuMuKstar.opts


//==============================================================================
// HLT Selection of B -> Mu Mu Kstar
//==============================================================================
// Author: P. Koppenburg,  29/10/2004

//------------------------------------------------------------------------------
// Filter All B -> MuMuKstar
//------------------------------------------------------------------------------
HLTSelections.Members += {"GaudiSequencer/HLTselBd2MuMuKstar"};
HLTselBd2MuMuKstar.IgnoreFilterPassed = false ; // act as real Sequencer
HLTselBd2MuMuKstar.MeasureTime = true ;

//------------------------------------------------------------------------------
// Filter dimuons
HLTselBd2MuMuKstar.Members += {"RefineSelection/HLTDimuonForBd2MuMuKstar"};   
HLTDimuonForBd2MuMuKstar.PhysDesktop.InputLocations = {"Phys/HLTDimuons"};
HLTDimuonForBd2MuMuKstar.ParticleNames = {"J/psi(1S)", "mu+", "mu-"};
HLTDimuonForBd2MuMuKstar.FilterNames = {"PsiFilter", "MuFilter", "MuFilter"};

HLTDimuonForBd2MuMuKstar.MuFilter.CriteriaNames = { "KinFilterCriterion" } ;
HLTDimuonForBd2MuMuKstar.MuFilter.KinFilterCriterion.MinPt = 900*MeV ; // offline: 900 MeV

HLTDimuonForBd2MuMuKstar.PsiFilter.CriteriaNames = { "VtxFilterCriterion"
                                             , "MassFilterCriterion"
					     , "FlightDistanceFilterCriterion" } ;
HLTDimuonForBd2MuMuKstar.PsiFilter.VtxFilterCriterion.MaxChi2 = 20;  // offline = 8
HLTDimuonForBd2MuMuKstar.PsiFilter.MassFilterCriterion.UpperWindow = 2100 ; // offline M<5.2GeV
HLTDimuonForBd2MuMuKstar.PsiFilter.MassFilterCriterion.LowerWindow = 3000 ; // no offline cut
HLTDimuonForBd2MuMuKstar.PsiFilter.FlightDistanceFilterCriterion.MinFSPV = 0.7 ; // offline: 1.5
// Offline one also rejects Psi and Psi' but we don't want this
// in the HLT

//------------------------------------------------------------------------------
// Filter K*
HLTselBd2MuMuKstar.Members += {"RefineSelection/HLTKstarForBd2MuMuKstar"};   
HLTKstarForBd2MuMuKstar.PhysDesktop.InputLocations = {"Phys/HLTKPiFromKstar"};
HLTKstarForBd2MuMuKstar.ParticleNames = {"K*(892)0", "K*(892)~0", "K+", "K-", "pi+", "pi-"};
HLTKstarForBd2MuMuKstar.FilterNames = {"KstarFilter", "KstarFilter", "KFilter", "KFilter", "PiFilter", "PiFilter"};

HLTKstarForBd2MuMuKstar.KFilter.CriteriaNames = { "PVIPFilterCriterion" } ;
HLTKstarForBd2MuMuKstar.KFilter.PVIPFilterCriterion.MinIPsignif = 1. ; // offline: 2 sigma

HLTKstarForBd2MuMuKstar.PiFilter.CriteriaNames = { "PVIPFilterCriterion"
                                           , "KinFilterCriterion" } ;
HLTKstarForBd2MuMuKstar.PiFilter.PVIPFilterCriterion.MinIPsignif = 1. ; // offline: 2 sigma
HLTKstarForBd2MuMuKstar.PiFilter.KinFilterCriterion.MinPt = 200. ; // offline: 200 MeV

HLTKstarForBd2MuMuKstar.KstarFilter.CriteriaNames = { "VtxFilterCriterion"
                                             , "MassFilterCriterion"
					     , "KinFilterCriterion"
					     , "FlightDistanceFilterCriterion" } ;
HLTKstarForBd2MuMuKstar.KstarFilter.VtxFilterCriterion.MaxChi2 = 20.;  // offline = 8
HLTKstarForBd2MuMuKstar.KstarFilter.MassFilterCriterion.Window = 150. ; // offline 100 MeV
HLTKstarForBd2MuMuKstar.KstarFilter.KinFilterCriterion.MinPt = 900. ; // offline 900 MeV
HLTKstarForBd2MuMuKstar.KstarFilter.FlightDistanceFilterCriterion.MinFSPV = 0.7 ; // offline: 1.5

//------------------------------------------------------------------------------
// Combine K* and dimuon
HLTselBd2MuMuKstar.Members += {"CombineParticles/HLTBd2MuMuKstar"};   
HLTBd2MuMuKstar.PhysDesktop.InputLocations = {"Phys/HLTDimuonForBd2MuMuKstar", "Phys/HLTKstarForBd2MuMuKstar"};
HLTBd2MuMuKstar.DecayDescriptor = "[B0 -> J/psi(1S) K*(892)0]cc";
HLTBd2MuMuKstar.Selections = {"B0 : BooleanFilterCriterion/Chi2Filter",
                                "B~0 : BooleanFilterCriterion/Chi2Filter",
                                "J/psi(1S) : BooleanFilterCriterion",
                                "K*(892)0 : BooleanFilterCriterion",
                                "K*(892)~0 : BooleanFilterCriterion"};

HLTBd2MuMuKstar.Chi2Filter.AndList = { "VtxFilterCriterion" } ;
HLTBd2MuMuKstar.Chi2Filter.VtxFilterCriterion.MaxChi2 = 200.; // offline: 10

//------------------------------------------------------------------------------
// Filter B
HLTselBd2MuMuKstar.Members += {"RefineSelection/HLTFilterBd2MuMuKstar"};   
HLTFilterBd2MuMuKstar.PhysDesktop.InputLocations = {"Phys/HLTBd2MuMuKstar"};
HLTFilterBd2MuMuKstar.ParticleNames = {"B0", "B~0"};
HLTFilterBd2MuMuKstar.FilterNames = {"BFilter", "BFilter"};

HLTFilterBd2MuMuKstar.BFilter.CriteriaNames = { "MassFilterCriterion"
                                           , "PVIPFilterCriterion" } ;
HLTFilterBd2MuMuKstar.BFilter.MassFilterCriterion.Window = 500*MeV;
//HLTFilterBd2MuMuKstar.BFilter.PVIPFilterCriterion.MaxIPSignif = 100. ; // Offline: 3.5



// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBd2MuMuKstar.opts

//==============================================================================
// HLT Selection of B_s0 -> J/Psi(Mu+ Mu-) K*
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBs2JpsiPhi.opts


//==============================================================================
// HLT Selection of B -> J/psi Phi
//==============================================================================
// Author: P. Koppenburg,  29/10/2004

//------------------------------------------------------------------------------
// Filter All Bs -> J/psi Phi
//------------------------------------------------------------------------------
HLTSelections.Members += {"GaudiSequencer/HLTselBs2JpsiPhi"};
HLTselBs2JpsiPhi.IgnoreFilterPassed = false ; // act as real Sequencer
HLTselBs2JpsiPhi.MeasureTime = true ;

//------------------------------------------------------------------------------
// Filter Psis
HLTselBs2JpsiPhi.Members += {"RefineSelection/HLTJpsiForBs2JpsiPhi"};   
HLTJpsiForBs2JpsiPhi.PhysDesktop.InputLocations = {"Phys/HLTAllJpsis"};
HLTJpsiForBs2JpsiPhi.ParticleNames = {"J/psi(1S)", "mu+", "mu-"};
HLTJpsiForBs2JpsiPhi.FilterNames = {"PsiFilter", "MuFilter", "MuFilter"};

HLTJpsiForBs2JpsiPhi.MuFilter.CriteriaNames = { "KinFilterCriterion" } ;
HLTJpsiForBs2JpsiPhi.MuFilter.KinFilterCriterion.MinPt = 500*MeV ; // offline: 500 MeV

HLTJpsiForBs2JpsiPhi.PsiFilter.CriteriaNames = { "VtxFilterCriterion"
                                             , "MassFilterCriterion" } ;
HLTJpsiForBs2JpsiPhi.PsiFilter.VtxFilterCriterion.MaxChi2 = 20;  // offline = 9
HLTJpsiForBs2JpsiPhi.PsiFilter.MassFilterCriterion.Window = 50 ; // offline 50 MeV

//------------------------------------------------------------------------------
// Filter Phi
HLTselBs2JpsiPhi.Members += {"RefineSelection/HLTPhiForBs2JpsiPhi"};   
HLTPhiForBs2JpsiPhi.PhysDesktop.InputLocations = {"Phys/HLTKKFromPhi"};
HLTPhiForBs2JpsiPhi.ParticleNames = {"phi(1020)", "K+", "K-"};
HLTPhiForBs2JpsiPhi.FilterNames = {"PhiFilter", "KFilter", "KFilter"};

HLTPhiForBs2JpsiPhi.KFilter.CriteriaNames = { "KinFilterCriterion" } ;
HLTPhiForBs2JpsiPhi.KFilter.KinFilterCriterion.MinPt = 500 ; // offline: 500 MeV

HLTPhiForBs2JpsiPhi.PhiFilter.CriteriaNames = { "VtxFilterCriterion"
                                         , "MassFilterCriterion" } ;
HLTPhiForBs2JpsiPhi.PhiFilter.VtxFilterCriterion.MaxChi2 = 100.;  // offline = 40
HLTPhiForBs2JpsiPhi.PhiFilter.MassFilterCriterion.Window = 30. ; // offline 20 MeV

//------------------------------------------------------------------------------
// Combine Phi and Psi
HLTselBs2JpsiPhi.Members += {"CombineParticles/HLTBs2JpsiPhi"};   
HLTBs2JpsiPhi.PhysDesktop.InputLocations = { "Phys/HLTJpsiForBs2JpsiPhi", "Phys/HLTPhiForBs2JpsiPhi"};
HLTBs2JpsiPhi.DecayDescriptor = "B_s0 -> J/psi(1S) phi(1020)";
HLTBs2JpsiPhi.Selections = {"B_s0 : BooleanFilterCriterion/Chi2Filter",
                                "J/psi(1S) : BooleanFilterCriterion",
                                "phi(1020) : BooleanFilterCriterion"};

HLTBs2JpsiPhi.Chi2Filter.AndList = { "VtxFilterCriterion" } ;
HLTBs2JpsiPhi.Chi2Filter.VtxFilterCriterion.MaxChi2 = 50.; // offline: 20

//------------------------------------------------------------------------------
// Filter B
HLTselBs2JpsiPhi.Members += {"RefineSelection/HLTFilterBs2JpsiPhi"};   
HLTFilterBs2JpsiPhi.PhysDesktop.InputLocations = {"Phys/HLTBs2JpsiPhi"};
HLTFilterBs2JpsiPhi.ParticleNames = {"B0", "B~0"};
HLTFilterBs2JpsiPhi.FilterNames = {"BFilter", "BFilter"};

HLTFilterBs2JpsiPhi.BFilter.CriteriaNames = { "MassFilterCriterion"
                                        , "PVIPFilterCriterion"
					, "FlightDistanceFilterCriterion" } ;
HLTFilterBs2JpsiPhi.BFilter.MassFilterCriterion.Window = 500*MeV;// Offline: 15
HLTFilterBs2JpsiPhi.BFilter.PVIPFilterCriterion.MaxIPSignif = 10. ; // Offline: 4.0
HLTFilterBs2JpsiPhi.BFilter.FlightDistanceFilterCriterion.MinFSPV = 0.; // offline: 5 (do we need this?)


// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBs2JpsiPhi.opts

//==============================================================================
// HLT Selection of any separated J/psi
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBd2JpsiKs.opts


//==============================================================================
// HLT Selection of B -> Mu Mu Phi
//==============================================================================
// Author: P. Koppenburg,  29/10/2004

//------------------------------------------------------------------------------
// Filter High IP J/psi 
//------------------------------------------------------------------------------
HLTSelections.Members += {"GaudiSequencer/HLTselBd2JpsiKs"};
HLTselBd2JpsiKs.IgnoreFilterPassed = false ; // act as real Sequencer
HLTselBd2JpsiKs.MeasureTime = true ;

//------------------------------------------------------------------------------
// Filter Psis
HLTselBd2JpsiKs.Members += {"RefineSelection/HLTHighIPJpsi"};   
HLTHighIPJpsi.PhysDesktop.InputLocations = {"Phys/HLTAllJpsis"};
HLTHighIPJpsi.ParticleNames = {"J/psi(1S)", "mu+", "mu-"};
HLTHighIPJpsi.FilterNames = {"PsiFilter", "MuFilter", "MuFilter"};

HLTHighIPJpsi.MuFilter.CriteriaNames = { "PVIPFilterCriterion" } ;
HLTHighIPJpsi.MuFilter.PVIPFilterCriterion.MinIPSignif = 0.7 ; // offline: 1.4

HLTHighIPJpsi.PsiFilter.CriteriaNames = { "VtxFilterCriterion"
                                     , "MassFilterCriterion" 
				     , "FlightDistanceFilterCriterion"} ;
HLTHighIPJpsi.PsiFilter.VtxFilterCriterion.MaxChi2 = 50;  // offline = 20
HLTHighIPJpsi.PsiFilter.MassFilterCriterion.Window = 100 ; // offline = 50 MeV
HLTHighIPJpsi.PsiFilter.FlightDistanceFilterCriterion.MinFSPV = 0.4 ; // offline = 1.2 in z


// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBd2JpsiKs.opts

//==============================================================================
// HLT Decision
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTDecision.opts


HLTSelections.Members += {"GaudiSequencer/HLTDecision"};
HLTDecision.IgnoreFilterPassed = true ;
HLTDecision.MeasureTime = true ;

// Exclusive flag
HLTDecision.Members += { "CheckSelResult/HLTExclusiveSelectionsFlag" } ;
HLTExclusiveSelectionsFlag.Algorithms = {  
                         "HLTFilterB2HH"
                       , "HLTFilterBd2D02KPiKstar"
                       , "HLTFilterBd2DstarPi"
                       , "HLTFilterBs2DsH"
                       , "HLTFilterBs2PhiPhi"
			,"HLTFilterBs2PhiGamma"		
			,"HLTHighIPJpsi"		
		       , "HLTFilterB2MuMu"
			,"HLTFilterBd2MuMuKstar"			
			,"HLTFilterBs2JpsiPhi"		
} ;
HLTExclusiveSelectionsFlag.Andmode = false ; 
HLTExclusiveSelectionsFlag.OutputLevel = 3 ;

// J/psi flag
HLTDecision.Members += { "CheckSelResult/HLTJpsi2MuMuFlag" } ;
HLTJpsi2MuMuFlag.Algorithms = {  "HLTHotJpsis" } ;
HLTJpsi2MuMuFlag.OutputLevel = 3 ;

// Hot muon flag
HLTDecision.Members += { "CheckSelResult/HLTHotMuonFlag" } ;
HLTHotMuonFlag.Algorithms = {  "HLTHotMuons" } ;
HLTHotMuonFlag.OutputLevel = 3 ;

// Dstar flag
HLTDecision.Members += { "CheckSelResult/HLTDstarFlag" } ;
HLTDstarFlag.Algorithms = {  "HLTFilterDstar" } ;
HLTDstarFlag.OutputLevel = 3 ;

// <-- End of including file $HLTSELECTIONSROOT/options/HLTDecision.opts


// <-- End of including file $HLTSELECTIONSROOT/options/HLTSelections.opts


