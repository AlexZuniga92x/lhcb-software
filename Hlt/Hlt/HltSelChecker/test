/** @file 
 *  Recommended options for D02KPiForTDRselBd2DstarPi algorithm for tight selection of
 *  D0 -> K+ pi- and their charged conjugates
 *
 *  @author L.Allebone
 *  @date 2004-04-19
 */

// Initialise algorithm properties (units are MeV/mm/nsec) 
D02KPiForTDRselBd2DstarPi.HistogramFlag          = true;   //
D02KPiForTDRselBd2DstarPi.NTupleFlag             = true;   //
D02KPiForTDRselBd2DstarPi.D0MassWindowTight      = 11;     // MeV  
D02KPiForTDRselBd2DstarPi.D0ZWindow              = 150;    // mm   
D02KPiForTDRselBd2DstarPi.PtPionCutLoose         = 150;    // MeV  
D02KPiForTDRselBd2DstarPi.PtKaonCutLoose         = 200;    // MeV  
D02KPiForTDRselBd2DstarPi.PtD0CutLoose	         = 50;     // MeV
D02KPiForTDRselBd2DstarPi.PionImpactParameter    = 0;      // mm   
D02KPiForTDRselBd2DstarPi.KaonImpactParameter    = 0;      // mm  
D02KPiForTDRselBd2DstarPi.CombVertexUnconstFit   = 40;     // mm   
D02KPiForTDRselBd2DstarPi.D0vtxMinusPrimVtxUpper = 200;    // mm   
D02KPiForTDRselBd2DstarPi.D0vtxMinusPrimVtxLower = 0;      // mm   
D02KPiForTDRselBd2DstarPi.PionConfLevelCut       = 0.0;    //      
D02KPiForTDRselBd2DstarPi.KaonConfLevelCut       = 0.0;    //      


//---------------------------------------------------------------------------
// Configure PhysDeskTop Tool with necessary values or values
// different from default
//---------------------------------------------------------------------------

D02KPiForTDRselBd2DstarPi.PhysDeskTop.InputLocations  = {"/Event/Phys/CombinedForTDRselBd2DstarPi"};


//---------------------------------------------------------------------------

//----------------------------------------------------------------------------
// Configure the Particle Transporter in the Velo Region 
// Possible options for this version are LinearTransporter or 
// NsctVeloTransporter
//----------------------------------------------------------------------------

D02KPiForTDRselBd2DstarPi.LagrangeMassVertexFitter.CombinedTransporter.VeloTransporterName =
					"NsctVeloTransporter";
D02KPiForTDRselBd2DstarPi.UnconstVertexFitter.CombinedTransporter.VeloTransporterName =
				 	"NsctVeloTransporter";
D02KPiForTDRselBd2DstarPi.GeomDispCalculator.CombinedTransporter.VeloTransporterName  = 
					"NsctVeloTransporter";


//----------------------------------------------------------------------------
// Configure the Particle Transporter outside the Velo Region 
// Possible options for this version are LinearTransporter or 
// ParabolicTransporter
//----------------------------------------------------------------------------
