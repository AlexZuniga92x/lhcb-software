/** @file 
 *  Recommended options for TDRselBd2Jpsi2MuMuKs algorithm for selection of
 *  Bd -> J/Psi (mu+mu-) Ks (pi+pi-) for Long tracks
 *
 *  @author S.Amato
 *  @date 2003-04-24
 */

TDRselBd2Jpsi2MuMuKsLong.HistogramFlag = false;
TDRselBd2Jpsi2MuMuKsLong.B0MassWindowLoose = 600.0;
//tight mass cut
//TDRselBd2Jpsi2MuMuKsLong.B0MassWindowTight = 60.0;
//loose mass cut
TDRselBd2Jpsi2MuMuKsLong.B0MassWindowTight = 600.0;
TDRselBd2Jpsi2MuMuKsLong.ImpParamB0 = 5.0;
TDRselBd2Jpsi2MuMuKsLong.ImpParamKsJPsi = 3.5;
TDRselBd2Jpsi2MuMuKsLong.MinImpParamPi = 3.0;
TDRselBd2Jpsi2MuMuKsLong.Chi2UncFitCut = 16.0;
TDRselBd2Jpsi2MuMuKsLong.MinImpParamMu = 1.35;
TDRselBd2Jpsi2MuMuKsLong.SigDistanceJpsi = 1.2;
TDRselBd2Jpsi2MuMuKsLong.SigDistanceKs = 5.7;
TDRselBd2Jpsi2MuMuKsLong.PtB = 200.;
TDRselBd2Jpsi2MuMuKsLong.ImpParamKsPV = 1.0;
TDRselBd2Jpsi2MuMuKsLong.KsLocation = "/Event/Phys/Ks2PiPiForTDRselBd2JpsiKsLong/Particles";
TDRselBd2Jpsi2MuMuKsLong.JpsiLocation = "/Event/Phys/TDRselJpsi2MuMu/Particles";
TDRselBd2Jpsi2MuMuKsLong.TThole = 0.0;

//-----------------------------------------------------------------
// Configure PhysDesktop Tool with necessary values or values
// different from default
//-----------------------------------------------------------------
TDRselBd2Jpsi2MuMuKsLong.PhysDesktop.InputLocations = {"/Event/Phys/TDRselJpsi2MuMu", 
                                                        "/Event/Phys/Ks2PiPiForTDRselBd2JpsiKsLong",
                                                        "/Event/Phys/Combined" };
TDRselBd2Jpsi2MuMuKsLong.DecayDescriptor = "B0 -> (KS0 -> pi+ pi-) (J/psi(1S) -> mu+ mu-)";
