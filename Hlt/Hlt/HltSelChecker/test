// --> Including file $HLTSELCHECKERROOT/options/TrueBd2DstarPi.opts


TrueSignals.Members += {"RefineSelection/SelectBd2DstarPi"};
SelectBd2DstarPi.PhysDesktop.InputLocations = { "Phys/HLTPions" } ;
SelectBd2DstarPi.ParticleNames =  { "pi-", "pi+"  } ;                           // select pi-, pi+
SelectBd2DstarPi.FilterNames = { "TrueMC", "TrueMC" };                         // filter by TrueMC
SelectBd2DstarPi.TrueMC.CriteriaNames =  { "TrueMCFilterCriterion/Bd2DstarPi" } ;   // 
SelectBd2DstarPi.TrueMC.Bd2DstarPi.VetoSignal = false; // default value is false, thus false keeps only signal
SelectBd2DstarPi.TrueMC.Bd2DstarPi.MCDecayFinder.Decay =  "{[[B0]nos -> ^pi+ (D*(2010)- -> ^pi- (D~0 -> ^K+ ^pi-))]cc, [[B0]os -> ^pi- (D*(2010)+ -> ^pi+ (D0 -> ^K- ^pi+))]cc}" ;

SelectBd2DstarPi.TrueMC.Bd2DstarPi.Particle2MCLinks.Location = "Phys/Relations/Particle2MCLinks" ;


// check that all 4 tracks are there
TrueSignals.Members += {"CombineParticles/AllBd2DstarPiTracks"};
AllBd2DstarPiTracks.PhysDesktop.InputLocations = {"Phys/SelectBd2DstarPi"};
AllBd2DstarPiTracks.DecayDescriptor = "B0 -> pi+ pi+ pi- pi-" ;
AllBd2DstarPiTracks.Selections = {  "B0 : BooleanFilterCriterion", 
                                    "pi+ : BooleanFilterCriterion", 
                                    "pi- : BooleanFilterCriterion"} ;

// <-- End of including file $HLTSELCHECKERROOT/options/TrueBd2DstarPi.opts

// --> Including file $HLTSELCHECKERROOT/options/TrueBs2DsH.opts


TrueSignals.Members += {"RefineSelection/SelectBs2DsH"};
SelectBs2DsH.PhysDesktop.InputLocations = { "Phys/HLTPions" } ;
SelectBs2DsH.ParticleNames =  { "pi-", "pi+"  } ;                           // select pi-, pi+
SelectBs2DsH.FilterNames = { "TrueMC", "TrueMC" };                         // filter by TrueMC
SelectBs2DsH.TrueMC.CriteriaNames = { "BooleanFilterCriterion/Bs2DsH" } ;   // 
SelectBs2DsH.TrueMC.Bs2DsH.OrList =  { "TrueMCFilterCriterion/DsPi",
                                       "TrueMCFilterCriterion/DsK" } ;   // 
SelectBs2DsH.TrueMC.Bs2DsH.DsPi.MCDecayFinder.Decay =  "{[[B_s0]nos -> (D_s- => ^K+ ^K- ^pi-) ^pi+]cc, [[B_s0]os -> (D_s+ => ^K- ^K+ ^pi+) ^pi-]cc}" ;
SelectBs2DsH.TrueMC.Bs2DsH.DsK.MCDecayFinder.Decay =  "{[[B_s0]nos -> (D_s- => ^K+ ^K- ^pi-) ^K+]cc, [[B_s0]os -> (D_s+ => ^K- ^K+ ^pi+) ^K-]cc}" ;
SelectBs2DsH.TrueMC.Bs2DsH.DsPi.Particle2MCLinks.Location = "Phys/Relations/Particle2MCLinks" ;
SelectBs2DsH.TrueMC.Bs2DsH.DsK.Particle2MCLinks.Location = "Phys/Relations/Particle2MCLinks" ;

// check that all 4 tracks are there
TrueSignals.Members += {"CombineParticles/AllBs2DsHTracks"};
AllBs2DsHTracks.PhysDesktop.InputLocations = {"Phys/SelectBs2DsH"};
AllBs2DsHTracks.DecayDescriptor = "B_s0 -> pi+ pi+ pi- pi-" ;
AllBs2DsHTracks.Selections = {   "B_s0 : BooleanFilterCriterion", 
                                  "pi+ : BooleanFilterCriterion", 
                                  "pi- : BooleanFilterCriterion"} ;


// <-- End of including file $HLTSELCHECKERROOT/options/TrueBs2DsH.opts

// --> Including file $HLTSELCHECKERROOT/options/TrueBs2PhiGamma.opts


TrueSignals.Members += {"RefineSelection/SelectBs2PhiGamma"};
SelectBs2PhiGamma.PhysDesktop.InputLocations = { "Phys/HLTPions" } ;// ignore gamma so far
SelectBs2PhiGamma.ParticleNames =  { "pi-", "pi+"  } ;                           // select pi-, pi+
SelectBs2PhiGamma.FilterNames = { "TrueMC", "TrueMC" };                         // filter by TrueMC
SelectBs2PhiGamma.TrueMC.CriteriaNames =  { "TrueMCFilterCriterion/Bs2PhiGamma" } ;   // 
SelectBs2PhiGamma.TrueMC.Bs2PhiGamma.MCDecayFinder.Decay =  "[B_s0 -> (phi(1020) -> ^K+ ^K-) gamma]cc" ;  // ignore gamma so far
SelectBs2PhiGamma.TrueMC.Bs2PhiGamma.Particle2MCLinks.Location = "Phys/Relations/Particle2MCLinks" ;

// check that all 2 tracks are there
TrueSignals.Members += {"CombineParticles/AllBs2PhiGammaTracks"};
AllBs2PhiGammaTracks.PhysDesktop.InputLocations = {"Phys/SelectBs2PhiGamma"};
AllBs2PhiGammaTracks.DecayDescriptor = "B0 -> pi+ pi-" ;// ignore gamma so far
AllBs2PhiGammaTracks.Selections = {   "B0 : BooleanFilterCriterion", 
                               "pi+ : BooleanFilterCriterion", 
                               "pi- : BooleanFilterCriterion"} ;

// <-- End of including file $HLTSELCHECKERROOT/options/TrueBs2PhiGamma.opts

// --> Including file $HLTSELCHECKERROOT/options/TrueB2MuMu.opts


TrueSignals.Members += {"RefineSelection/SelectB2MuMu"};
SelectB2MuMu.PhysDesktop.InputLocations = { "Phys/HLTPions" } ;
SelectB2MuMu.ParticleNames =  { "pi-", "pi+"  } ;                           // select pi-, pi+
