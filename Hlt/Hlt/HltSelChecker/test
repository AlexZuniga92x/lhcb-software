CombinedForBs2PhiPhi.PhysDesktop.CombinedParticleMaker.ExclusiveSelection =  
false ;
CombinedForBs2PhiPhi.PhysDesktop.CombinedParticleMaker.Particles          =  
  { "kaon" };
CombinedForBs2PhiPhi.PhysDesktop.CombinedParticleMaker.KaonSelection      =  
  { "det='RICH' k-pi='-2' "} ;

