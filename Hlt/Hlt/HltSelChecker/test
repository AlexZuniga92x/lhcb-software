ShowFoundB2HH.Members += { "CheckSelResult/B2HHAndTruth",  "PrintTree/PrintFoundB2HH"};

B2HHAndTruth.ANDmode = true ;
B2HHAndTruth.Algorithms = { "HLTFilterB2HH" , "AllB2HHTracks" } ;
B2HHAndTruth.AvoidSelResult = false ;

