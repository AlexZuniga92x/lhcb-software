/** @file 
 *  Options to execute the final reconstruction of ChargedProtoParticles
 *  and Primary Vertices.
 *  
 *  @author G.Corti
 *  @date 2004-02-03
 */

// Charged ProtoParticles
DaVinciMainSeq.Members += { "ChargedProtoPAlg" };

// Primary vertex
ApplicationMgr.DLLs += { "PrimVtx" };
// --> Including file $PRIMVTXROOT/options/PrimVtx.opts


PrimVtxFinder.OutputLevel               = 3;
PrimVertAnalysis.OutputLevel            = 4;
PrimVtxFinder.PrimVtxFitterTool.OutputLevel               = 3;
PrimVtxFinder.PrimVtxFitterTool.TrFirstCleverExtrapolator.OutputLevel = 3;
PrimVtxFinder.PrimVtxFitterTool.TrLinearExtrapolator.OutputLevel = 3;
PrimVtxFinder.TrLinearExtrapolator.OutputLevel = 3;

PrimVtxFinder.UseVTT       = true;
PrimVtxFinder.UseVelo      = true;
PrimVtxFinder.UseVeloBack  = true;
PrimVtxFinder.UseForward   = true;
PrimVtxFinder.UseMatch     = true;
PrimVtxFinder.PrimVtxFitterTool.Chi2Cut  = 9.;      // default is 16.
PrimVtxFinder.PrimVtxFitterTool.deWeight  = false;
PrimVtxFinder.PrimVtxFitterTool.MinTracks  = 6;

// <-- End of including file $PRIMVTXROOT/options/PrimVtx.opts

DaVinciMainSeq.Members += { "PrimVtxFinder" };


