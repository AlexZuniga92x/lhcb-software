package           RichRecSys
version           v1r13

# ================ structure ================================================
branches          cmt doc options

# ================ environment ==============================================
set RICHRECSYSOPTS $(RICHRECSYSROOT)/options

# ================ dependencies =============================================

# DAQ Decoding
use RichDAQ         v1r6   Rich

# Common RICH reconstruction packages
use RichRecBase     v1r14  Rich
use RichRecTools    v2r12  Rich
use RichRecCommon   v1r10  Rich

# "Offline" PID packages
use RichGlobalPID   v1r8   Rich
use RichLocalPID    v1r9   Rich
use RichRingRefit   v1r9   Rich

# Online PID packages
use RichTrgTools      v1r0   Rich
use RichTrgLocalPID   v1r0   Rich
use RichTrgGlobalPID  v1r0   Rich

# Result merging algorithms
use RichPIDMerge    v1r5   Rich

# MC packages
use RichRecMCTools  v1r7   Rich

