package MuonID
version v4r0

branches src cmt doc options

# include directory
include_dirs $(MUONIDROOT)

# add "use" statements
use GaudiAlg           v*
use RecEvent           v* Event
use TrackEvent         v* Event
use MuonDet            v* Det
use ROOT               v* LCG_Interfaces -no_auto_imports



# build a MuonID library
library MuonID ../src/*.cpp

apply_pattern component_library library=MuonID
