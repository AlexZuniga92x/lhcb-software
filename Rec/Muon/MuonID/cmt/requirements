package MuonID
version v3r7p1

branches src cmt doc options

# include directory
include_dirs $(MUONIDROOT)

# add "use" statements
use MuonEvent          v3r*  Event
use MuonTools          v3r*  Muon

# build a MuonID library
library MuonID ../src/*.cpp

apply_pattern component_library library=MuonID
