#! /bin/csh -f
#------------------------------
# Make Doxygen documentation
#------------------------------

source ../cmt/setup.csh

if ( -d $DaVinci_release_area/DAVINCI/doc/html) then
  mv $DaVinci_release_area/DAVINCI/doc/html $DaVinci_release_area/DAVINCI/doc/html_prev
endif

$GAUDISOFT/../extsoft/doxygen-1.2.5/bin/doxygen DoxyFile.cfg

