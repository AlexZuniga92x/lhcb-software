     // placeholder for "output" jets 
     IJetMaker::Jets   jets ;
   
     // find the jets! 
     StatusCode sc = jetMaker -> makeJets ( input , jets ) ;
   
     // make  a loop over jets:
     for ( IJetMaker::Jets::const_iterator iJet = jets.begin() ; 
