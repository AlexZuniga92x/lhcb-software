  echo moving $I
  mv DaVinciTools/$I Kernel/
  echo "#ifdef linux" > DaVinciTools/$I
  echo "#warning" >> DaVinciTools/${I}
  echo "#warning Header files in DaVinciKernel/DaVinciTools/ moved to DaVinciKernel/Kernel/" >> DaVinciTools/$I
