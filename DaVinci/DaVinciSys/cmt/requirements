package DaVinciSys
version v36r2

branches doc cmt

# Packages
use  DaVinci         v36r2    Phys
use  DaVinciTests    v2r27
use  KaliCalo        v4r16    Phys   # Olivier Deschamps
use  Tesla           v1r0     Phys   # Sean Benson

# Temporary packages ...
# Remember to remove from CMake file as well ...
use  CaloPIDs        v5r20    Calo

# Declare this as a container package
apply_pattern container_package

# For running tests
alias DaVinciTests "cmt TestProject"

# Allow the generation of the SAM QMTest
apply_pattern QMTest

# Allow the generation of QMTest summary 
apply_pattern QMTestSummarize
