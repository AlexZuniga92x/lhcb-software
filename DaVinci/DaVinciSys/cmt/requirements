package DaVinciSys
version v36r5

branches doc cmt

# Packages
use  DaVinci         v36r5    Phys   # DaVinci coordinator
use  DaVinciTests    v2r30
use  KaliCalo        v4r17    Phys   # Olivier Deschamps
use  Tesla           v1r3     Phys   # Sean Benson

# Extra package just for this release,
# to deal with seg faults seen when reading Strip21 MC
use  EventPacker     v5r0     Event

# Declare this as a container package
apply_pattern container_package

# For running tests
alias DaVinciTests "cmt TestProject"

# Allow the generation of the SAM QMTest
apply_pattern QMTest

# Allow the generation of QMTest summary 
apply_pattern QMTestSummarize
